display: block;
padding: .75rem 1.25rem;
margin-bottom: -1px;
background-color: #fff;
border: 1px solid rgba(0,0,0,.125);
}
ul{padding-left:0;}
.badge-secondary {
background: #fc5846;
padding: 15px;
border-radius: 5px;    color: #fff;
width: 100%;
}

.thanks {
text-align:left;
display: inherit;
padding-top: 2px;
font-size:17px;
color: blueviolet;

}
.my-0{font-size: 15px;}
.text-center{text-align: center; padding:15px 0;}
.material-icons{color:#fff;}
.badge-pill a{color:#fff;}
.text-right{text-align: right;}
.pull-right{float:right;}
span{vertical-align: top;}
.side-add{max-width: 300px;
float: right;
position: fixed;
display: flex;
top: 130px;
right: 19px;}
.name-comp{ float: left; margin:2px 0;padding:5px;    font-size: 19px; text-transform: uppercase; font-size: 16px !important;}
#icon{background: #fff;}
#icon .material-icons{color: #055ba9  !important; padding-top:0px !important; float: left;}
.badge-secondary{ border-radius: 0;}

@media(max-width:576px){
  #icon{font-size:12px;}
 .material-icons{font-size:14px;}
 .paym small, span{font-size: 15px !important;
  padding-top: 2px !important;
  text-transform: uppercase !important;
  clear: both;
  display: inline-block;
  text-align: left !important; }
}
@media(max-width:400px){
  .mat-toolbar span{display: none  !important;}
}
  @media (max-width:699px){
    .container{padding:2px 5px;}
    .mat-toolbar span{margin-top:18px !important;}
    .premim-mob{place-content: center !important;}
  }
  p{padding:16px 0;}
  .container{padding: 4px 11px 10px !important;}
  #icon span{font-size:20px; text-align: center;}
  .badge-secondary{padding: 10px 7px 3px 8px; text-transform: capitalize !important;}
/* ******* LOADER CSS ***************** */

.loader,
.loader:before,
.loader:after {
  border-radius: 50%;
  width: 2.5em;
  height: 2.5em;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  -webkit-animation: load7 1.8s infinite ease-in-out;
  animation: load7 1.8s infinite ease-in-out;
}
.loader {
  color: #F53100;
  font-size: 10px;
  margin: 80px auto;
  position: relative;
  text-indent: -9999em;
  -webkit-transform: translateZ(0);
  -ms-transform: translateZ(0);
  transform: translateZ(0);
  -webkit-animation-delay: -0.16s;
  animation-delay: -0.16s;
}
.loader:before,
.loader:after {
  content: '';
  position: absolute;
  top: 0;
}
.loader:before {
  left: -3.5em;
  -webkit-animation-delay: -0.32s;
  animation-delay: -0.32s;
}
.loader:after {
  left: 3.5em;
}

@-webkit-keyframes load7 {
  0%,
  80%,
  100% {
    box-shadow: 0 2.5em 0 -1.3em;
  }
  40% {
    box-shadow: 0 2.5em 0 0;
  }
}
@keyframes load7 {
  0%,
  80%,
  100% {
    box-shadow: 0 2.5em 0 -1.3em;
  }
  40% {
    box-shadow: 0 2.5em 0 0;
  }
}

:host ::ng-deep .mat-expansion-panel-header-title {
  font-weight: bold;
  color:#595858!important;
}

.loader-wrap{ 
  margin:-85px auto;
  left: 50%;
  text-align: center;
}
