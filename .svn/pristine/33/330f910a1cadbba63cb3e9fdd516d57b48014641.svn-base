.mat-form-field-label-wrapper{top: -1.8em !important;
  margin-top: 2px;}
  .mat-form-field-appearance-legacy .mat-form-field-wrapper {
      padding-top: 20px !important;
  }
.mat-expansion-panel-header-description{flex:none;}
.mat-icon {
  background-repeat: no-repeat;
  display: inline-block;
  fill: currentColor;
  height: 34px;
  width: 23px;
  margin-left: -12px;
  margin-right: 6px;
  font-size: 22px;
}
.mat-expansion-panel-header-description, .mat-expansion-panel-header-title{font-weight: bold;color: #470394;}
.dob{position: absolute; font-weight: bold; color: #999; font-size: 11px;}
.warn{margin:15px auto !important;}
.justify-content-between {
justify-content: space-between!important;
}

.d-flex {
