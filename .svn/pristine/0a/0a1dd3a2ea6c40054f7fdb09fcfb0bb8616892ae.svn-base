<ng-template #thirdPartyModelCover>
   <h2 matDialogTitle>
      Additional Covers
      <mat-chip-list style="border: none;">
         <a style="text-decoration: none;border: none;" href="JavaScript:void(0);" (click)="onNoClick()"><i
            class="material-icons">
         close
         </i></a>
      </mat-chip-list>
   </h2>
   <ul class="addons">
      <li *ngFor="let p of thirdPartyCover;let i=index">
         <mat-checkbox *ngIf="p.name!='NA'" [checked]="p.isChecked" (change)="thirdPartyCoverContrl($event,i);">{{p.name}}
         </mat-checkbox>
         <br *ngIf="pa_cover_checked && i==2">
         <mat-form-field *ngIf="pa_cover_checked && i==2">
            <mat-select [(ngModel)]="pa_cover" (ngModelChange)="changePaCover($event)">
            <mat-option [value]="100000" selected>SI :100000</mat-option>
            <mat-option [value]="200000">SI :200000</mat-option>
            </mat-select>
         </mat-form-field>
         <br *ngIf="pa_paid_checked && i==0">
         <mat-form-field *ngIf="pa_paid_checked && i==0">
            <mat-select [(ngModel)]="paid_cover" (ngModelChange)="changePaidCover($event)">
            <mat-option [value]="100000" selected>SI :100000</mat-option>
            <mat-option [value]="200000">SI :200000</mat-option>
            </mat-select>
         </mat-form-field>
      </li>
   </ul>
   <mat-form-field *ngIf="thirdPartyCover[3].isChecked">
      <mat-select [(ngModel)]="legal_liability" (ngModelChange)="change_legal_liability_paid_driver($event)">
      <mat-option [value]="1">1</mat-option>
      <mat-option [value]="2">2</mat-option>
      </mat-select>
   </mat-form-field>
   <h2 matDialogTitle>Discounts</h2>
   <ul class="addons">
      <li class="pull-left" style="width: 100%">
         <label>TPPD Discount: </label>
         <mat-radio-group>
            <mat-radio-button [checked]="is_tppd_deduct" (click)="changeTPPD(2)"  value="1">Yes</mat-radio-button>
            <mat-radio-button [checked]="!is_tppd_deduct" (click)="changeTPPD(1)"  value="0">No</mat-radio-button>
         </mat-radio-group>
      </li>
   </ul>
</ng-template>
<ng-template #garagelist>
   <div id="pbm" fxLayout="column">
      <div fxFlex="100" fxLayout="column" >
         <h3 style="text-align:center;
            color:rgb(5, 91, 169) ;
            font-size: 18px !important; text-transform: uppercase; font-weight: bold;
            ">
            Garages in your city
            <mat-chip-list style="border: none;">
               <a style="text-decoration: none;border: none;" href="JavaScript:void(0);" (click)="onNoClick()" area-label="close"><i
                  class="material-icons">
               close
               </i></a>
            </mat-chip-list>
         </h3>
         <div fxFlex="95" fxLayout="row" style="margin-left:10px;">
            <div fxFlex="40" style="margin-right:10px;">
               <mat-form-field>
                  <mat-label [ngClass]="{'setLabelRed':setStateErr==true}">Select State</mat-label>
                  <input autocomplete="off" matInput [formControl]="myStateControl" [matAutocomplete]="auto"
                  (input)="resetStateErr($event.target.value)">
                  <mat-autocomplete #auto="matAutocomplete">
                     <mat-option *ngFor="let option of filteredOptionsState | async" [value]="option.sname"
                     (click)="setState(option.id,option.sname);getCity(option.id)">
                     {{option.sname}}
                     </mat-option>
                  </mat-autocomplete>
               </mat-form-field>
            </div>
            <div fxFlex="45">
               <mat-form-field>
                  <mat-label [ngClass]="{'setLabelRed':setCityErr==true}">Select City</mat-label>
                  <input autocomplete="off" matInput [formControl]="myCityControl" [matAutocomplete]="auto2"
                  (input)="resetCityErr($event.target.value)">
                  <mat-autocomplete #auto2="matAutocomplete">
                     <mat-option *ngFor="let option of filteredOptionsCity | async"
                     [value]="option.city_name" (click)="setCity(option.city_id,option.city_name,option.state_id);">
                     {{option.city_name}}
                     </mat-option>
                  </mat-autocomplete>
               </mat-form-field>
            </div>
         </div>
         <div class="mat-button-wrapper loader-wrap" *ngIf="garageLoader">
            <div class="loader">Loading...</div>
         </div>
         <div  fxFlex="100" fxLayout="row" *ngIf="garargelistbool && !garageLoader" fxLayout.xs="column">
            <mat-dialog-content style="width: 97%; overflow-x: hidden;" class="mat-typography scrollbar scrollbar-near-moon thin">
               <div *ngFor="let item of garargelistfilter" style="border-bottom:1px solid #ccc; margin-bottom: 10px; margin-top:10px;">
                  <div class="breakUp" fxLayout.xs="column">
                     <div fxFlex="65%"
                        style="font-weight: bold; font-size:14px !important; color:rgb(2, 124, 92); float: left;">{{item.name}}
                     </div>
                     <div fxFlex="35%"  fxFlex.xs="75" class="breakupFlex" style="float:right;">
                        <span class="font-bold">
                        <i class="material-icons" style="float: left;">
                        local_phone
                        </i> {{item.phn}}</span>
                     </div>
                  </div>
                  <div class="breakUp">
                     <div fxFlex="100%" style="padding-top: 0 !important;">
                        <h2 matDialogTitle style="padding-top: 0 !important; font-size: 13px !important;">{{item.addr}}
                        </h2>
                     </div>
                  </div>
               </div>
            </mat-dialog-content>
         </div>
      </div>
   </div>
</ng-template>
<ng-template #share>
   <h2 matDialogTitle>
      Share This Plan
      <mat-chip-list style="border: none;">
         <a style="text-decoration: none;border: none;" href="JavaScript:void(0);" (click)="onNoClick()"><i class="material-icons">close</i></a>
      </mat-chip-list>
   </h2>
   <mat-form-field class="example-full-width">
      <input matInput placeholder=""  value="{{quoteUrl}}" #refferalLink readonly>
   </mat-form-field>
   <div style="text-align: center;">
      <button mat-raised-button color="primary" (click)="copyToClipboard(refferalLink);">
      <span *ngIf="!shareFormCopyMsg">Copy</span><span *ngIf="shareFormCopyMsg">Copied</span>
      </button>
   </div>
   <form [formGroup]="shareFormEmail" (ngSubmit)="shareEmailFormSubmit()">
   <mat-form-field class="example-full-width">
      <input matInput placeholder="Enter email address" formControlName="refEmailAddress"  value="">
   </mat-form-field>
   <div class="example-button-row nprow text-center" style="text-align: center;float:left; width:100%">
      <button type="submit" mat-raised-button color="warn" class="info-bottom" > <span>Send Email</span></button>
      <p *ngIf="shareFormEmailMsg">Email sucessfully sent to customer.</p>
   </div>
   </form>
   <form [formGroup]="shareFormSMS" (ngSubmit)="shareSMSFormSubmit()">
   <mat-form-field class="example-full-width">
      <input matInput placeholder="Enter phone no" formControlName="refMobileNo"  value="">
   </mat-form-field>
   <div class="example-button-row nprow text-center" style="text-align: center;float:left; width:100%">
      <button type="submit" mat-raised-button color="warn" class="info-bottom" > Send SMS</button>
      <p *ngIf="shareFormSMSMsg">Sms sucessfully sent to customer.</p>
   </div>
   </form>
</ng-template>
<ng-template #short>
   <mat-dialog-actions>
      <h2 matDialogTitle style="width: 100%;">
         Sort By
         <mat-chip-list style="border: none;">
            <a style="text-decoration: none;border: none;" href="JavaScript:void(0);" (click)="onNoClick()" area-label="close"><i
               class="material-icons">
            close
            </i></a>
