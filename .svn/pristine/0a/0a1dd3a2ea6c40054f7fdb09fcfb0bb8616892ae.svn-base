         <mat-panel-title>
            Discounts &nbsp;
            <i matTooltip="{{getMoreInformation()}}" matTooltipClass="example-tooltip-red mat-tooltip-while-space"
               aria-label="Button that shows a red tooltip" class="material-icons mat-help-tolltip">help_outline</i>
         </mat-panel-title>
         <mat-panel-description>
            {{panelOpenState ? '' : ''}}
         </mat-panel-description>
      </mat-expansion-panel-header>
      <form class="example-container" [formGroup]="quoteModifyLeftForm">
         <div class="pull-left" style="width: 100%">
            <label>Voluntary Discount: </label>
            <mat-radio-group>
               <mat-radio-button [checked]="is_voluntary_deduct" (click)="changeSelected(2500,2)" value="1">
               Yes</mat-radio-button>
               <mat-radio-button [checked]="!is_voluntary_deduct" (click)="changeSelected(0,1)" value="0">
               No</mat-radio-button>
            </mat-radio-group>
         </div>
         <div *ngIf="is_voluntary_deduct" class="mat-chip-list-wrapper"
            style="padding: 7px 15px; float:left;width: 100%">
            <mat-chip-list>
               <mat-chip color="primary" [selected]="quoteJson.voluntary_deduct_am==2500" (click)="changeSelected(2500)">
               Rs 2500</mat-chip>
               <mat-chip color="primary" [selected]="quoteJson.voluntary_deduct_am==5000" (click)="changeSelected(5000)">
               Rs 5000</mat-chip>
               <mat-chip color="primary" [selected]="quoteJson.voluntary_deduct_am==7500" (click)="changeSelected(7500)">
               Rs 7500</mat-chip>
               <mat-chip color="primary" [selected]="quoteJson.voluntary_deduct_am==15000"
               (click)="changeSelected(15000)">Rs 15000</mat-chip>
            </mat-chip-list>
         </div>
         <div class="pull-left" style="width: 100%">
            <label>Want Occupation Discount: </label>
            <mat-radio-group>
               <mat-radio-button [checked]="quoteJson.occupationDiscount" (click)="quoteJson.occupationDiscount=true"
               value="1">Yes</mat-radio-button>
               <mat-radio-button [checked]="!quoteJson.occupationDiscount" (click)="quoteJson.occupationDiscount=false"
               value="0">No</mat-radio-button>
            </mat-radio-group>
         </div>
         <div *ngIf="quoteJson.occupationDiscount" style="width: 100%">
            <mat-form-field>
               <mat-label>Occupation Type</mat-label>
               <mat-select (selectionChange)="occupationTypeDiscount($event.value)">
                  <mat-option>None</mat-option>
                  <mat-option value="option1">Professional Practicing Chartered Accountant</mat-option>
                  <mat-option value="option2">Central / State Government Employees</mat-option>
                  <mat-option value="option3">Doctors registered with Government</mat-option>
                  <mat-option value="option4">Defense and Para Military Service</mat-option>
                  <mat-option value="option5">Teacher in Govt. recognized Institutions</mat-option>
               </mat-select>
            </mat-form-field>
         </div>
         <div class="pull-left" style="width: 100%">
            <label>Want Age Discount: </label>
            <mat-radio-group>
               <mat-radio-button [checked]="ageDiscount" (click)="ageDiscount=true" value="1">Yes
               </mat-radio-button>
               <mat-radio-button [checked]="!ageDiscount" (click)="ageDiscount=false" value="0">No
               </mat-radio-button>
            </mat-radio-group>
         </div>
         <div *ngIf="ageDiscount" style="width: 100%">
            <mat-form-field>
               <mat-label>DOB</mat-label>
               <input matInput [matDatepicker]="picker" placeholder="Choose a date">
               <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
               <mat-datepicker #picker></mat-datepicker>
            </mat-form-field>
         </div>
         <div class="pull-left" style="width: 100%">
            <label>Want Motor Association Discount: </label>
            <mat-radio-group>
               <mat-radio-button [checked]="motorAssociationDiscountFlag" (click)="motorAssociationDiscount(true)"
               value="1">Yes</mat-radio-button>
               <mat-radio-button [checked]="!motorAssociationDiscountFlag" (click)="motorAssociationDiscount(false)"
               value="0">No</mat-radio-button>
            </mat-radio-group>
         </div>
         <div *ngIf="motorAssociationDiscountFlag" style="width: 100%">
            <p>
               <mat-form-field appearance="standard">
                  <mat-label>Association Name</mat-label>
                  <input matInput [(ngModel)]="quoteJson.motorAssociationDiscountValue[0].association_Name"
                  [ngModelOptions]="{standalone: true}">
               </mat-form-field>
            </p>
            <p>
               <mat-form-field appearance="standard">
                  <mat-label>Membership No.</mat-label>
                  <input matInput [(ngModel)]="quoteJson.motorAssociationDiscountValue[1].membership_No"
                  [ngModelOptions]="{standalone: true}">
               </mat-form-field>
            </p>
            <p>
               <mat-form-field appearance="standard">
                  <mat-label>Expiry Date</mat-label>
                  <input matInput [min]="minmemberShipDate" [(ngModel)]="quoteJson.motorAssociationDiscountValue[2].expiry_Date"
                  [ngModelOptions]="{standalone: true}" [matDatepicker]="picker" placeholder="Choose a date">
                  <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
                  <mat-datepicker #picker></mat-datepicker>
               </mat-form-field>
            </p>
         </div>
         <div class="pull-left" style="width: 100%">
            <label>Anti-theft Device(ARAI): </label>
            <mat-radio-group>
               <mat-radio-button [checked]="AntiTheftDeviceDiscount" (click)="setAntiTheftDeviceDiscount(true)"
               value="1">Yes</mat-radio-button>
               <mat-radio-button [checked]="!AntiTheftDeviceDiscount" (click)="setAntiTheftDeviceDiscount(false)"
               value="0">No</mat-radio-button>
            </mat-radio-group>
         </div>
         <div class="pull-left" style="width: 100%">
            <label>Handicapped Discount: </label>
            <mat-radio-group>
               <mat-radio-button [checked]="HandicappedDiscount" (click)="setHandicappedDiscount(true)"
               value="1">Yes</mat-radio-button>
               <mat-radio-button [checked]="!HandicappedDiscount" (click)="setHandicappedDiscount(false)"
               value="0">No</mat-radio-button>
            </mat-radio-group>
         </div>
         <div class="pull-left" style="width: 100%">
            <label>TPPD Discount: </label>
            <mat-radio-group>
               <mat-radio-button [checked]="is_tppd_deduct" (click)="changeTPPD(2)"  value="1">Yes</mat-radio-button>
               <mat-radio-button [checked]="!is_tppd_deduct" (click)="changeTPPD(1)"  value="0">No</mat-radio-button>
            </mat-radio-group>
         </div>
      </form>
      </mat-expansion-panel>
   </mat-dialog-content>
   <!-- <div *ngIf="recalculateBtnShowHide>0" style="text-align: center;">
      <button mat-raised-button color="primary" (click)="recalculate()">Recalculate
      </button>
   </div> -->

   <div style="text-align: center;">
      <button mat-raised-button color="primary" (click)="recalculate()">
         Recalculate
      </button>
   </div>
</ng-template>

