<header>
   <div fxFlexFill style="background: #055ba9;">
      <div fxLayout="row" class="container">
         <mat-toolbar color="primary">
            <header fxFlexFill fxFlex.lt-sm="100%">
               <div fxLayoutAlign="left" fxFlex="20" class="one" (click)="homepage()">
                  <img *ngIf="white_label==0" src="https://www.gibl.in/UI/assets/quote/images/logo.png" alt="logo" height="67">
                  <!-- <span fxHide.xs   fxLayoutAlign="right" style="padding-left: 10px;"> CAR QUOTES </span> -->
                  <small style="padding:5px 10px; font-size: 12px; text-transform: uppercase;">Car Quotes</small>
               </div>
               <div fxFlex fxLayout  fxLayoutAlign="flex-end" style="float: right; font-size: 11px; margin-top: 4px; cursor: pointer;" fxFlex="85" fxShow.gt-sm >
                  <div *ngIf="quoteJson && quoteJson.quoteID>0"  fxLayout="row" fxLayout.xs="column" fxShow.gt-sm >
                     <i (click)="generateLink(); openDialog(share);" style="padding-top: 6px; margin-top: -2px;"><img src="assets/quotecompare/images/share-white-18dp.svg">
                     <small style="padding: 12px 5px;line-height: 2px;float: right;font-size: 12px;font-style: normal; text-transform: uppercase  ;">Share Quote</small>
                     </i>
                  </div>
               </div>
               <ng-template #planType>
                  <mat-dialog-actions>
                     <div fxLayout="row" fxFlex="100">
                        <button mat-button [matMenuTriggerFor]="menu">Support</button>
                        <button mat-button (click)="openDialog(moo3)">Login</button>
                        <button mat-button [matMenuTriggerFor]="menu">Register</button>
                     </div>
                  </mat-dialog-actions>
               </ng-template>
            </header>
         </mat-toolbar>
      </div>
   </div>
</header>

<div class="container" fxHide.xs fxHide.sm  >
   <div fxLayout="row wrap">
      <div fxLayoutAlign="center" class="new-head" style="flex-basis:100%;">
         <div class="container wrap-contact101 no-padding mt-10 header newpad tt" fxLayout="row" fxLayout.xs="column" fxLayout.sm="column" fxFlexFill fxFlex="" fxFlex.sm="100" style="padding-top:10px !important">
            <ng-container *ngIf="show2">
               <div fxLayout="column" class="" fxFlex="48" fxFlex.xs="100" fxFlex.sm="100" fxLayoutAlign="flex-start"
                  style="align-items:baseline;">
                  <!-- <p>{{brand_name}} {{model_name}} {{variant_name}} <a (click)="goBackToQuote()" area-label="edit"><i
                     class="material-icons edit-n">edit</i></a>
                     </p> -->
                  <p>{{brand_name}} {{model_name}} {{variant_name}} <a (click)="openDialognow(contenteditcar);track_button('EDIT-BIKE-DETAILS');" area-label="edit"><i
                     class="material-icons edit-n">edit</i></a>
                  </p>
                  <p><b>CC :</b> {{car_cc}} | {{fuel_type_text}} {{rtoText}}</p>
				  <div class="custom-checkbox" style="padding-left: 5px !important;" *ngIf="B2B_AND_B2C_USER_TYPE.includes(+userJson?.role_type) && userJson?.white_label=='0'">
					<input type="checkbox" [checked]="quoteJson?.b2b && quoteJson?.b2b==1" (change)="changeB2B($event);" class="custom-control-input" id="b2blist">
					<label  class="custom-control-label" style="margin-left: 24px;" for="b2blist">Use My Wallet</label>
				 </div>
               </div>
            </ng-container>
            <div fxFlex="33" fxLayout="column" class="sec3" fxFlex.xs="50" fxFlex.sm="50" fxFlex.md="80"
               fxLayoutAlign="flex-basis">
               <p>Registration Date: <b>{{registration_date_text}}</b> <a (click)="openMoidifyDialog(modifyPopUp);track_button('EDIT-DATE-DETAILS')" area-label="edit"><i
                  class="material-icons edit-n">
                  edit
                  </i></a>
               </p>
               <p *ngIf="form_premium_type!=0 && quoteJson?.dont_have_prev_policy==false">Previous Policy Expiry Date: <b>{{pre_policy_expiry_date_text}}</b></p>
               <p fxHide fxShow.xs fxShow.sm style="font-size: 14px !important;font-weight: bold;padding-top: 12px;">
                  <span
                     (click)="toggle2()" id="bt" class="text-size3 text-right show-add" fxHide>
                     Edit Other Detail
                     <mat-icon style="font-size: 30px; padding-right: 3px;float: right;top: -14px;">
                        {{!show2 ? 'keyboard_arrow_down' : 'keyboard_arrow_up'}}
                     </mat-icon>
                     {{panelshow}}
                  </span>
               </p>
            </div>
            <ng-template #modifyPopUp >
               <h2 matDialogTitle>
                  Modify Your Car's Information
                  <mat-chip-list style="border: none;">
                     <a style="text-decoration: none;border: none;" href="JavaScript:void(0);" (click)="onNoClick()" area-label="close"><i
                        class="material-icons">
                     close
                     </i></a>
                  </mat-chip-list>
               </h2>
               <mat-dialog-content style="width: 97%" class="mat-typography scrollbar scrollbar-near-moon thin">
                  <form [formGroup]="quoteModifyForm" (ngSubmit)="quoteModifySubmit()">
                  <div class="edit-detail">
                     <mat-accordion [multi]="true">
                        <div class="edit-pad">
                           <p>Registration Date & Prev. Policy Expiry Date</p>
                           <mat-expansion-panel [expanded]="true" >
                              <div fxLayout="row" fxFlex="100"  class="edit-detail" fxLayout.xs="column">
                                 <mat-form-field fxFlex="45"  style="margin-right: 15px;" fxFlex.xs="45">
                                    <input [min]="quoteJson.regMinDate" [max]="quoteJson.regMaxDate" (click)="dp.open()" matInput
                                    [matDatepicker]="dp" placeholder="Registration Date" formControlName="modifyRegistrationDate"
                                    (dateChange)="updateRegDate($event)">
                                    <mat-datepicker-toggle matSuffix [for]="dp"></mat-datepicker-toggle>
                                    <mat-datepicker #dp></mat-datepicker>
                                 </mat-form-field>
                                 <div fxFlex="10"></div>
                                 <mat-form-field *ngIf="form_premium_type!=0 && quoteJson?.dont_have_prev_policy==false" fxFlex="45" fxFlex.xs="45" >
                                    <input (click)="picker.open()" [min]="quoteJson.minExpDate" [max]="quoteJson.maxExpDate" matInput formControlName="modifyExpiryDate" [matDatepicker]="picker"
                                    placeholder="Previous Policy Experiry Date">
                                    <mat-datepicker-toggle matSuffix [for]="picker"></mat-datepicker-toggle>
                                    <mat-datepicker #picker></mat-datepicker>
                                 </mat-form-field>
                              </div>
                           </mat-expansion-panel>
                        </div>
                        <!-- <mat-expansion-panel [expanded]="true">
                           <mat-expansion-panel-header>
                           	<mat-panel-description ></mat-panel-description>
                           </mat-expansion-panel-header>

                           </mat-expansion-panel> -->
                        <div class="edit-pad">
                           <p>Manufacturing month & year</p>
                           <mat-expansion-panel [expanded]="true">
                              <div fxLayout="row" fxFlex="100" class="edit-detail">
                                 <mat-form-field fxFlex="45">
                                    <mat-select placeholder="Month" formControlName="modifyManufacDateMM">
                                       <mat-option [value]='1'>January</mat-option>
                                       <mat-option [value]='2'>February</mat-option>
                                       <mat-option [value]='3'>March</mat-option>
                                       <mat-option [value]='4'>April</mat-option>
                                       <mat-option [value]='5'>May</mat-option>
                                       <mat-option [value]='6'>June</mat-option>
                                       <mat-option [value]='7'>July</mat-option>
                                       <mat-option [value]='8'>August</mat-option>
                                       <mat-option [value]='9'>September</mat-option>
                                       <mat-option [value]='10'>October</mat-option>
                                       <mat-option [value]='11'>November</mat-option>
                                       <mat-option [value]='12'>December</mat-option>
                                    </mat-select>
                                 </mat-form-field>
                                 <div fxFlex="10"></div>
                                 <mat-form-field fxFlex="45">
                                    <mat-select placeholder="Year" formControlName="modifyManufacDateYY">
                                       <mat-option *ngFor="let year of maufacYear" [value]="year">{{year}}</mat-option>
                                    </mat-select>
                                 </mat-form-field>
                                 <mat-error *ngIf="showErrorModifyModalMsg">Car manufacture date must be less than registration date.</mat-error>
                              </div>
                           </mat-expansion-panel>
                        </div>
                        <div class="edit-pad">
                           <mat-expansion-panel [expanded]="true" *ngIf="prev_policy_type_reg_popup=='C' && form_premium_type!=0 && !quoteJson.isThirdParty && (!same_provider_bool || (same_provider_bool && NUM_NCB_PREM>0))">
                           <mat-radio-group>
                              <p>Claim in the past year</p>
                              <mat-radio-button [checked]="last_claim==1" value="1" (click)="changePastClaim('1')">Yes</mat-radio-button>
                              <mat-radio-button [checked]="last_claim==0" value="0" (click)="changePastClaim('0')">No</mat-radio-button>
                           </mat-radio-group>
                           <div *ngIf="last_claim==0" class="mat-chip-list-wrapper" style="float: left;width: 100%;margin-left: 1px !important;">
                              <p>Previous No Claim Bonus</p>
                              <div fxFlex="100">
                                 <div style="float: left;">	<label style="padding-right: 19px; padding-top:10px;">NCB Declaration</label> </div>
                                 <div style="float: left;margin-left:8px;">
                                    <mat-chip-list>
                                       <mat-chip *ngFor="let ncb_val of ncb_array" [selected]="prev_ncb==ncb_val" (click)="changeNCB(ncb_val)" color="primary">{{ncb_val}}%</mat-chip>
                                    </mat-chip-list>
                                 </div>
                              </div>
                           </div>
                           </mat-expansion-panel>
                           <mat-expansion-panel [expanded]="true" *ngIf="prev_policy_type_reg_popup=='C' && form_premium_type!=0 && !quoteJson.isThirdParty && (!same_provider_bool || (same_provider_bool && NUM_NCB_PREM>0))" [expanded]="edit_field=='NCB'">
                           <small style="padding:15px 0; float: left;">If the NCB declaration is found to be wrong, policy would be converted to TP only and pro rata refund of OD premium would be initiated - after appropriate notice / letter to customers.</small>
                           </mat-expansion-panel>
                        </div>
                        <div class="edit-pad" fxLayout.xs="column">
                           <div class="half" fxflex="50" fxflex.xs="50" fxLayout.xs="column" >
                              <mat-expansion-panel [expanded]="true" *ngIf="!quoteJson.isThirdParty && form_premium_type!=0">
                              <p>Previous Policy Type</p>
                              <mat-radio-group [(value)]="quoteJson.pre_policy_type">
                              <mat-radio-button value="COMPREHENSIVE" (change)="changePrevPolicyType_reg_popup('C')">Comprehensive
                              </mat-radio-button>
                              <mat-radio-button value="THIRDPARTY" (change)="changePrevPolicyType_reg_popup('T')">Third Party
                              </mat-radio-button>
                              </mat-radio-group>
                              <div *ngIf="prev_policy_type_reg_popup=='T' && form_premium_type!=0" style="color: red;font-size: 13px;padding-bottom: 10px;">Inspection will be required before purchase of comprehensive policy.
                              </div>
                              </mat-expansion-panel>
                           </div>
                           <div class="half" fxflex="50" fxflex.xs="50" >
                              <mat-expansion-panel [expanded]="true" *ngIf="same_provider_bool && prev_policy_type_reg_popup=='C' && form_premium_type!=0 && !quoteJson.isThirdParty && provider_id!=18">
                              <p>Is Your Previous Insurer {{company_name}}</p>
                              <mat-radio-group>
                                 <mat-radio-button [checked]="same_provider==1" value="1" (click)="changePrevInsurer(provider_id)">Yes</mat-radio-button>
                                 <mat-radio-button [checked]="same_provider==0" value="0" (click)="changePrevInsurer('0')">No</mat-radio-button>
                              </mat-radio-group>
                              <p *ngIf="same_provider==1">Please choose a different insurer?
                                 <br>
                                 Your existing policy is with {{company_name}}. This insurer does not allow you to purchase a {{company_name}} policy on GIBL. We're sorry for this tiny bit of inconvenience.
                              </p>
                              </mat-expansion-panel>
                           </div>
                           <div class="example-button-row nprow text-center" style="text-align: center;float:left; width:100%">
                              <button (click)="track_button('INFO-CORRECT');" type="submit" mat-raised-button color="warn" class="info-bottom" style="font-size: 21px !important;
                              padding: 5px 24px;" > Information is correct</button>
                           </div>
                        </div>
                     </mat-accordion>
                  </div>
                  </form>
               </mat-dialog-content>
            </ng-template>
            <ng-template #contenteditcar>
               <h2 matDialogTitle>
                  Edit Car Details
                  <mat-chip-list style="border: none;">
                     <a style="text-decoration: none;border: none;" href="JavaScript:void(0);" (click)="onNoClick()" area-label="edit"><i
                        class="material-icons">
                     close
                     </i></a>
                  </mat-chip-list>
               </h2>
               <mat-dialog-content style="width: 97%" class="mat-typography scrollbar scrollbar-near-moon thin">
                  <form  class="idv-pop">
                     <mat-dialog-actions>
                        <mat-form-field>
                           <mat-label>Choose Brand </mat-label>
                           <input autocomplete="off" matInput [value]="brand_name" [formControl]="brandControl" [matAutocomplete]="autoBrand" placeholder="Select your brand">
                           <!--<input autocomplete="off" matInput [value]="brand_name" [matAutocomplete]="autoBrand" placeholder="Select your brand">-->
                           <mat-autocomplete #autoBrand="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsBrand | async" [value]="option.name" [id]="option.id" (onSelectionChange)="get_model($event.source.id)">
                              {{option.name}}
                              </mat-option>
                              <!--<mat-option *ngFor="let option of totalBrandList" [value]="option.name" [id]="option.id" (onSelectionChange)="get_model($event.source.id)">
                                 {{option.name}}
                                 </mat-option>-->
                           </mat-autocomplete>
                        </mat-form-field>
                        <mat-form-field>
                           <mat-label >Choose Model</mat-label>
                           <input autocomplete="off" matInput [value]="model_name" [formControl]="modelControl" [matAutocomplete]="autoModel" placeholder="Select your model">
                           <!--<input autocomplete="off" matInput [value]="model_name"  [matAutocomplete]="autoModel" placeholder="Select your model">-->
                           <mat-autocomplete #autoModel="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsModel | async" [value]="option.name" [id]="option.id" (onSelectionChange)="get_fuel($event.source.id)">
                              {{option.name}}
                              </mat-option>
                              <!--<mat-option *ngFor="let option of totalModelList" [value]="option.name" [id]="option.id" (onSelectionChange)="get_fuel($event.source.id)">
                                 {{option.name}}
                                 </mat-option>-->
                           </mat-autocomplete>
                        </mat-form-field>
                        <mat-form-field>
                           <mat-label >Choose Fuel</mat-label>
                           <input autocomplete="off" matInput [value]="fuel_name" [matAutocomplete]="autoFuel">
                           <mat-autocomplete #autoFuel="matAutocomplete">
                              <mat-option *ngFor="let option of totalFuelList" [value]="option.fuel_type_text" (onSelectionChange)="get_variant($event.source.id)" [id]="option" >
                              {{option.fuel_type_text}}
                              </mat-option>
                           </mat-autocomplete>
                        </mat-form-field>
                        <mat-form-field>
                           <mat-label >Choose Variant</mat-label>
                           <input autocomplete="off" [value]="variant_name" matInput [formControl]="variantControl" [matAutocomplete]="autoVariant" placeholder="Select your variant">
                           <!--<input autocomplete="off" [value]="variant_name" matInput [matAutocomplete]="autoVariant" placeholder="Select your variant">-->
                           <mat-autocomplete #autoVariant="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsVariant | async" [value]="option.name" [id]="option.id" (onSelectionChange)="get_car_details($event.source.id)">
                              {{option.name}}
                              </mat-option>
                              <!--<mat-option *ngFor="let option of totalVariantList" [value]="option.name" [id]="option.id" (onSelectionChange)="get_car_details($event.source.id)">
                                 {{option.name}}
                                 </mat-option>-->
                           </mat-autocomplete>
                        </mat-form-field>
                        <mat-form-field>
                           <mat-label >Choose RTO</mat-label>
                           <input autocomplete="off" [value]="rtoText" matInput  [formControl]="rtoControl" [matAutocomplete]="autoRto" placeholder="Select RTO (e.g. MH02)">
                           <mat-autocomplete #autoRto="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsRto | async" [value]="option.name" (search)="getrtoJson()" [id]="option.id"
                              (close)="getrtoJson()" (onSelectionChange)="rtoTapped('',$event.source.id);">
                              {{option.name}}
                              </mat-option>
                           </mat-autocomplete>
                        </mat-form-field>
                     </mat-dialog-actions>
                     <div class="text-center" style="margin:15px auto;" *ngIf="car_id>0 || form_rto_id>0">
                        <button type="button" (click)="change_fw()"  mat-raised-button color="warn" class="">Save Changes</button>
                        <button type="button" mat-button (click)="onNoClick()">No Thanks</button>
                     </div>
                  </form>
               </mat-dialog-content>
            </ng-template>
            <div *ngIf="quoteJson && !quoteJson.isThirdParty" fxFlexLayout="column" class="mob-addons" fxHide.md fxHide.lg>
               <ul class="selectbtn">
                  <li>
                     <button mat-raised-button (click)="openDialog(moo)" class="white">
                        Add Covers & More
                        <mat-icon
                           class="no-icon add-arrow pull-right" aria-hidden="false" aria-label="add material-icons">
                           add_circle_outline
                        </mat-icon>
                     </button>
                  </li>
               </ul>
            </div>
            <div *ngIf="quoteJson && quoteJson.isThirdParty" fxFlexLayout="column" class="mob-addons" fxHide.md fxHide.lg>
               <ul class="selectbtn">
                  <li>
                     <button mat-raised-button (click)="openDialog2(thirdPartyModelCover)" class="white">
                        Add Covers & More
                        <mat-icon class="no-icon add-arrow pull-right" aria-hidden="false" aria-label="add material-icons">
                           add_circle_outline
                        </mat-icon>
                     </button>
                  </li>
               </ul>
            </div>
            <ng-container *ngIf="show2 && quoteJson">
               <div *ngIf="!quoteJson.isThirdParty && form_premium_type!=0 && quoteJson.pre_policy_type != 'THIRDPARTY'" fxFlex="19" fxLayout="column"
                  class="sec3 border" fxFlex.xs="50" fxFlex.sm="50" fxLayout.sm="column">
                  <p style="margin-top:-5px;">Claim on Last Year: <b>{{claim_Previous_Policy}}</b> <a (click)="openMoidifyDialog(modifyPopUp,'NCB');track_button('EDIT-DATE-DETAILS');" area-label="edit"><i class="material-icons edit-n">edit</i></a>
                  </p>
                  <p>Previous NCB: <b *ngIf="claim_Previous_Policy=='Yes'">NA</b> <b *ngIf="claim_Previous_Policy!='Yes'">{{prev_ncb}}%</b></p>
               </div>
               <div *ngIf="quoteJson.isThirdParty || quoteJson.pre_policy_type == 'THIRDPARTY'"  fxFlex="19" fxLayout="column"
                  class="sec3 border" fxFlex.xs="50" fxFlex.sm="50" fxLayout.sm="column">
                  <p>Claim on Last Year: <b>NA</b></p>
                  <p>Previous NCB: <b>NA</b> </p>
               </div>
               <div fxFlexLayout="flex-end" class="text-right" fxFlex="25" fxFlex.xs="100" fxFlex.sm="50" fxFlex.md="50" class="text-size1">
                  <mat-radio-group>
                     <p>Policy Type: </p>
                     <mat-radio-button value="Comprehensive" *ngIf="IS_LIVE!=0" [checked]="quoteJson.premium_type=='COM'" (click)="changePrevPolicyType('Comprehensive',1)" >Comprehensive</mat-radio-button>
                     <mat-radio-button value="thirdParty" [checked]="quoteJson.premium_type=='TP'" (click)="changePrevPolicyType('thirdParty',1)">ThirdParty</mat-radio-button>
					      <mat-radio-button value="saOD" [checked]="quoteJson.premium_type=='SAOD'" (click)="changePrevPolicyType('saOD',1)">SAOD</mat-radio-button>
                  </mat-radio-group>
                  <!--<button  mat-raised-button color="warn" class="price" (click)="generateLink(); open(share);" class="white shr-btn">Share Quote</button>-->
               </div>
            </ng-container>
         </div>
      </div>
   </div>
</div>
<!--saheli -->
<div class="mobile_bottom_bar is-hidden-desktop-custom bottom_mobile_bar" fxShow.xs fxHide.md fxHide.lg>
   <div class="mobile_bottom_chat" (click)="openDialog(short)" fxFlex="44">
      <img src="assets/listing/images/short.jpg">
      <span class="bottom_bar_span">Sort</span>
   </div>
   <div class="mobile_bottom_chat" *ngIf="quoteJson" fxShow.xs fxHide.md fxHide.lg fxFlex="50">
      <div class="filter_mobile_quote_icon"  *ngIf="!quoteJson.isThirdParty" mat-raised-button (click)="openDialog(moo)">
      <div class="mobile_bottom_chat">
         <mat-icon class="no-icon add-arrow pull-right" aria-hidden="false" aria-label="add material-icons">
            add_circle_outline
         </mat-icon>
      </div>
      <span class="bottom_bar_span">Add Covers</span>
   </div>
   <div class="filter_mobile_quote_icon"  *ngIf="quoteJson.isThirdParty" mat-raised-button (click)="openDialog2(thirdPartyModelCover)" class="white">
   <div class="mobile_bottom_chat">
      <mat-icon class="no-icon add-arrow pull-right" aria-hidden="false" aria-label="add material-icons">
         add_circle_outline
      </mat-icon>
   </div>
   <span class="bottom_bar_span">Add Covers</span>
</div>
</div>
</div><!--saheli -->
<div class="container hide-div ">
   <div fxLayout="row wrap">
      <div fxFlex="100" fxLayout="column" fxHide.lg fxHide.md style="background: #0e5392;
         color: #fff; margin-bottom: 9px;">
         <div class="container top-panel-mob" style="width: 90%;
            font-size: 11px;
            line-height: 0;
            padding-bottom: 10px;" >
            <ng-container >
               <div fxLayout="column" fxLayout.xs="column" class="info-car" fxFlex="40" fxFlex.xs="100" fxFlex.sm="50" fxLayoutAlign="flex-start"
                  style="align-items:baseline;">
                  <p >{{brand_name}} {{model_name}} {{variant_name}} <a (click)="openDialognow(contenteditcar);track_button('EDIT-BIKE-DETAILS');" area-label="edit"><i
                     class="material-icons edit-n">edit</i></a>
                  </p>
                  <p ><b>CC</b> : {{car_cc}} | {{fuel_type_text}} {{rtoText}}</p>
               </div>
            </ng-container>
         </div>
      </div>
      <div fxLayoutAlign="center" class="new-head">
         <div class="container top-panel-mob"  fxLayout="row" fxHide.lg fxHide.md style="padding:4x; " fxFlex="93">
            <div fxFlex="40" class="top-button plantype">
               <span>Plan Type</span>
               <button mat-button (click)="openDialog(planType)" >Comprehensive <span class="material-icons">
               edit
               </span></button>
               <ng-template #planType>
                  <mat-dialog-actions>
                     <h2 matDialogTitle style="width: 100%;">
                        Choose the Best Plan Type
                        <mat-chip-list style="border: none;">
                           <a style="text-decoration: none;border: none;" href="JavaScript:void(0);" (click)="onNoClick()" area-label="close"><i
                              class="material-icons">
                           close
                           </i></a>
                        </mat-chip-list>
                     </h2>
                     <div fxLayout="row" fxFlex="100">
                        <mat-radio-button value="Comprehensive" [checked]="!quoteJson.isThirdParty" (click)="changePrevPolicyType('Comprehensive',1)" >Comprehensive</mat-radio-button>
                        <mat-radio-button value="thirdParty" [checked]="quoteJson.isThirdParty" (click)="changePrevPolicyType('thirdParty',1)">ThirdParty</mat-radio-button>
                     </div>
                  </mat-dialog-actions>
               </ng-template>
            </div>
            <div *ngIf="quoteJson" fxFlex="26" class="top-button">
               <span>Edit NCB</span>
