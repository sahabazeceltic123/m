<button [@modalButton]="isShow" *ngIf="isShowOffer" mat-fab="" color="none" aria-label="Example icon button with a bookmark icon" class="mat-focus-indicator mat-fab mat-button-base mat-accent" (click)="offerModalOpen()" style="position: fixed;
    background-image: url(assets/quote/img/auto-icon.png); background-size: contain; background-color: #fff; width: 132px; height: 138px; background-repeat:no-repeat;z-index: 1;" class="offer-click auto-position">
</button>


<!-- *ngIf="isShow" -->
<div class="myModal" [@content]="isShow" [ngClass]="modalClass">
   <div class="content my-modal-container" style="width: auto; border-radius: 45px;width: auto; max-width: 425px; border-radius: 45px;">
      <div>
         <button mat-fab="" color="none" aria-label="Example icon button with a bookmark icon" class="mat-focus-indicator mat-button-base mat-accent"
            style="float: right; background-color: #8d8d8dc6; margin: -56px -40px 0 0; color: #fff; border-radius: 50%;" (click)="offerModalClose()"> <!--  mat-fab  -->
            X
         </button>
      </div>
      <div class="container">
         <div fxFlex="100" *ngIf="autoOfferContentImgOrText">
            <img style="cursor: pointer; max-width:100%; border-radius: 6%;" src="assets/quote/img/win.jpg" (click)="showImgOrTestData(false)">
         </div>
         <div fxFlex="100" *ngIf="!autoOfferContentImgOrText">
            <img style="cursor: pointer; max-width:100%; border-radius: 6%;" src="assets/quote/img/step.jpg">

            <div class="example-button-row" style="float:right;">
               <button mat-raised-button color="warn" (click)="redirctCampaignPage();" style="    padding: 0 18px;
               border-radius: 12px;
               overflow: visible;
               transform: translate3d(0,0,0);
               transition: background .4s cubic-bezier(.25,.8,.25,1),box-shadow 280ms cubic-bezier(.4,0,.2,1);
               margin-top: 13px;">Play Now & Win</button>
            </div>
         </div>
      </div>
   </div>
</div>

<div class="my-loader" *ngIf="!isReady">
  Loading....
</div>
