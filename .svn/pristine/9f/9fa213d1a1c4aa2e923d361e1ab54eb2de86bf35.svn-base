<div  class="quoteBodyBackground" [style.background]="'url(assets/quote/img/city.jpg)'">
   <header>
      <div fxFlexFill [ngStyle]="{'background-color': (user_code=='0')?'transparent':((user_code=='150855') ? '#0EA4FF' : '#055ba9')}">
         <div fxLayout="row" class="container">
            <mat-toolbar color="primary">
               <header fxFlexFill fxFlex.lt-sm="50%" >
                  <div style="cursor: pointer;" *ngIf="showlogo" fxLayoutAlign="left" fxFlex="20" class="one">
                     <img *ngIf="white_label==0" (click)="homepage()" src="https://www.gibl.in/UI/assets/quote/images/logo.png" alt="logo" height="67">
                     <span fxLayoutAlign="right"> CAR INSURANCE </span>
                     <!--<div fxLayout="row" fxShow>
                        <button (click)="homepage()" *ngIf="white_label!=0" fxShow mat-button>HOME</button>
                        </div>-->
                  </div>
                  <div fxFlex fxLayout fxLayoutAlign="flex-end" fxFlex="85" fxHide fxShow.gt-sm style="margin-top: 5px;">
                     <div fxLayout="row" fxHide fxShow.gt-sm  *ngIf="white_label==0">
                        <button fxHide fxShow.gt-sm mat-button [matMenuTriggerFor]="life">
                          Life Insurance
                           <mat-icon fxLayoutalign="right" aria-hidden="false" aria-label="Example keyboard_arrow_down icon">
                              keyboard_arrow_down
                           </mat-icon>
                        </button>
                        <button fxHide fxShow.gt-sm mat-button [matMenuTriggerFor]="menu">
                           Insurance Type
                           <mat-icon fxLayoutalign="right" aria-hidden="false" aria-label="Example keyboard_arrow_down icon">
                              keyboard_arrow_down
                           </mat-icon>
                        </button>
                        <button fxHide fxShow.gt-sm mat-button [matMenuTriggerFor]="menuCom">
                           Insurance Company
                           <mat-icon fxLayoutalign="right" aria-hidden="false" aria-label="Example keyboard_arrow_down icon">
                              keyboard_arrow_down
                           </mat-icon>
                        </button>
                        <button (click)="supportLink()" fxHide fxShow.gt-sm mat-button>Support</button>
                        <!-- <button fxHide fxShow.gt-sm mat-button *ngIf="!isLoggedIn" (click)="openDialogLongin()">
                           LOGIN
                           </button> -->
                        <button fxHide fxShow.gt-sm mat-button *ngIf="!isLoggedIn"  Style="color: #fff!important;
                        background: #fb542f;
                        border-radius: 5px;
                        font-size: 11px;
                        padding: 9px 10px 10px;
                        margin-right: 5px;
                        ">
                        <a href="https://www.gibl.in/login.php" mat-button>Partner Login</a>
                        </button>
                        <button fxHide fxShow.gt-sm mat-button *ngIf="isLoggedIn"  Style="color: #fff!important;background: #fb542f;border-radius: 5px;font-size: 11px;padding:9px 10px 10px; margin-right:5px;">
                        <a href="https://www.gibl.in/logout.php" mat-button>
                        {{'Logout'  }}
                        </a>
                        </button>
                        <button fxHide fxShow.gt-sm mat-button *ngIf="!isLoggedIn" (click)="registrationPage()"  Style="color: #fff!important;background: #fb542f;border-radius: 5px;font-size: 11px;padding:9px 10px 10px; margin-right:5px;">
                        <a href="https://www.gibl.in/pos/" mat-button>{{'Partner Registration'}}</a>
                        </button>
                     </div>
                  </div>
                  <a class="nav-mob" fxHide *ngIf="white_label==0" (click)="openDialog()" area-label="storage"><i class="material-icons">
                  storage
                  </i>
                  </a>
                  <ng-template #moo3>
                     <mat-dialog-actions >
                        <div fxLayout="row" fxFlex="100" *ngIf="white_label==0">
                           <button mat-button [matMenuTriggerFor]="menu">
                              Insurance Type
                              <mat-icon fxLayoutalign="right" aria-hidden="false" aria-label="Example keyboard_arrow_down icon">
                                 keyboard_arrow_down
                              </mat-icon>
                           </button>
                           <button mat-button [matMenuTriggerFor]="menuCom">
                              Insurance Company
                              <mat-icon fxLayoutalign="right" aria-hidden="false" aria-label="Example keyboard_arrow_down icon">
                                 keyboard_arrow_down
                              </mat-icon>
                           </button>
                           <button mat-button [matMenuTriggerFor]="menu" >Support</button>
                           <button mat-button *ngIf="!isLoggedIn">
                           <a href="https://www.gibl.in/login.php" mat-button>Login</a>
                           </button>
                           <button mat-button (click)="logout()" *ngIf="isLoggedIn">Logout</button>
                           <button mat-button [matMenuTriggerFor]="menu" *ngIf="!isLoggedIn">Register</button>
                        </div>
                     </mat-dialog-actions>
                  </ng-template>
               </header>
            </mat-toolbar>
         </div>
      </div>
   </header>
   <mat-menu x-position="before" #life="matMenu">
      <button mat-menu-item><a href="https://www.gibl.in/life-insurance/life-insurance-plans/term-plans/">Term Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/life-insurance/life-insurance-plans/retirement-plans/">Retirement  Plans</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/life-insurance/life-insurance-plans/child-plans/">Child Plans</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/life-insurance/life-insurance-plans/ulip-plans/">ULIPS</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/life-insurance/life-insurance-plans/group-plans/">Group Plans</a></button>
   </mat-menu>
   <mat-menu x-position="before" #menu="matMenu">
      <button mat-menu-item><a href="https://www.gibl.in/car-insurance/">Car Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/health-insurance/">Health Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/two-wheeler-insurance/">Two Wheeler Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/travel-insurance/">Travel Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/personal-accident-insurance/">Personal Accident
      Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/home-insurance/">Home Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/commercial-vehicle-insurance/">Commercial Vehicle
      Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/life-insurance/">Life Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/health-insurance/critical-illness-insurance/">Critical Illness
      Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/gadget-insurance/">Gadget Insurance</a></button>
   </mat-menu>
   <mat-menu x-position="before" #menuCom="matMenu">
      <button mat-menu-item><a href="https://www.gibl.in/national-insurance/">National Insurance</a></button>
      <button mat-menu-item><a href="https://www.gibl.in/new-india-assurance/">New India Assurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/united-india-insurance-uiic/">United India Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/hdfc-ergo-insurance/">HDFC ERGO General Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/star-health-insurance/">Star Health Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/oriental-insurance/">Oriental Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/bajaj-allianz-insurance/">Bajaj Allianz General Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/iffco-tokio-insurance/">Iffco Tokio General Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/sbi-general-insurance/">SBI General Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/max-bupa-insurance/">MaxBupa Health insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/apollo-munich-insurance/">Apollo Munich Health Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/bharti-axa-insurance/">Bharti AXA General Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/tata-aig-insurance/">Tata AIG Greneral Insurance</a></button>
      <button mat-menu-item><a  href="https://www.gibl.in/universal-sompo-insurance/">Universal Sompo General Insurance </a></button>
   </mat-menu>

   <div class="containerX" >
      <div class="container" fxLayout fxLayout.xs="column" fxLayout.sm="column" fxLayoutAlign="left" fxLayoutGap=""
         fxLayoutGap.xs="0">
         <div class="item item-2" *ngIf="makeActive" fxFlex="50%" fxFlex.lt-md="100%" fxFlexOrder="1">
            <h1 style="font-weight: bold;">Car Insurance</h1>
            <!-- <h3 class="hidden-xs" [ngClass.lt-md]="{'mob-txt':true}">Compare Quotes from 25+ Car Insurance Companies in India</h3>
               <h4 [ngClass.lt-md]="{'mob-txt':true}">A great rate is just a few clicks away!</h4> -->
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 15px;
               font-weight: bold;">Compare Quotes from 25+ Car Insurance Companies in India</p>
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 8px;
               font-weight: bold;">A great rate is just a few clicks away!</p>
            <p *ngIf="form_premium_type==1" class="motor_banner_p" fxHide="true" fxShow.gt-sm>Renew your car insurance policy in less than 2 minutes with GIBL<br>Please select the brand of your car to begin.</p>
            <p *ngIf="form_premium_type==0" class="motor_banner_p" fxHide="true" fxShow.gt-sm>Congratulations on your new car! Let us find you the best insurance quotes.</p>
            <form class="example-container" [formGroup]="quoteForm">
               <div class="pdding-bottom-1">
                  <mat-radio-group formControlName="isRenewal">
                     <div *ngIf="IS_LIVE==0 || IS_LIVE==1">
                        <mat-radio-button (click)="isNewCar(0)" value="0" [checked]="form_premium_type==0">I've bought a new Car</mat-radio-button>
                        <mat-radio-button (click)="isNewCar(1)" value="1" [checked]="form_premium_type==1">Renewal</mat-radio-button>
                     </div>
                     <div *ngIf="IS_LIVE==2">
                        <mat-radio-button (click)="isNewCar(0)" value="0" [checked]="form_premium_type==0">I've bought a new Car</mat-radio-button>
                        <mat-radio-button (click)="isNewCar(1)" value="1" [checked]="form_premium_type==1">Renewal</mat-radio-button>
                     </div>
                  </mat-radio-group>
               </div>
            </form>
         </div>
         <div class="item item-3" *ngIf="modelActive" fxFlex="50%" fxFlex.lt-md="100%" fxFlexOrder="1">
            <!-- <h3 [ngClass.lt-md]="{'mob-txt':true}">Compare Quotes from 25+ Car Insurance Companies in India</h3>
               <h4 [ngClass.lt-md]="{'mob-txt':true}">A great rate is just a few clicks away!</h4> -->
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 15px;
               font-weight: bold;">Compare Quotes from 25+ Car Insurance Companies in India</p>
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 8px;
               font-weight: bold;">A great rate is just a few clicks away!</p>
            <p class="motor_banner_p" fxHide="true" fxShow.gt-sm > Your choosing of the right car model will help us find the right coverage for you
            </p>
         </div>
         <div class="item item-4" *ngIf="fuelActive" fxFlex="50%" fxFlexOrder="1">
            <!-- <h3 [ngClass.lt-md]="{'mob-txt':true}">Compare Quotes from 25+ Car Insurance Companies in India</h3>
               <h4 [ngClass.lt-md]="{'mob-txt':true}">A great rate is just a few clicks away!</h4> -->
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 15px;
               font-weight: bold;">Compare Quotes from 25+ Car Insurance Companies in India</p>
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 8px;
               font-weight: bold;">A great rate is just a few clicks away!</p>
            <p class="motor_banner_p" fxHide="true" fxShow.gt-sm>
               Now tell us what type of fuel your car engine uses
            </p>
         </div>
         <div class="item item-5" *ngIf="variantActive" fxFlex="50%" fxFlex.lt-md="100%" fxFlexOrder="1">
            <!-- <h3 [ngClass.lt-md]="{'mob-txt':true}">Compare Quotes from 25+ Car Insurance Companies in India</h3>
               <h4 [ngClass.lt-md]="{'mob-txt':true}">A great rate is just a few clicks away!</h4> -->
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 15px;
               font-weight: bold;">Compare Quotes from 25+ Car Insurance Companies in India</p>
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 8px;
               font-weight: bold;">A great rate is just a few clicks away!</p>
            <p class="motor_banner_p" fxHide="true" fxShow.gt-sm>
               Choose the correct variant of your car from the list.<br>
               You can manually type your car’s variant if it’s not visible in the list.
            </p>
         </div>
         <div class="item item-6" *ngIf="registrationActive" fxFlex="50%" fxFlex.lt-md="100%" fxFlexOrder="1">
            <!-- <h3 [ngClass.lt-md]="{'mob-txt':true}">Compare Quotes from 25+ Car Insurance Companies in India</h3>
               <h4 [ngClass.lt-md]="{'mob-txt':true}">A great rate is just a few clicks away!</h4> -->
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 15px;
               font-weight: bold;">Compare Quotes from 25+ Car Insurance Companies in India</p>
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 8px;
               font-weight: bold;">A great rate is just a few clicks away!</p>
            <p class="motor_banner_p" fxHide="true" fxShow.gt-sm>The city or town that your car is registered in can affect the amount of car insurance premium. Make sure you select the right RTO from the list.
            </p>
         </div>
         <div class="item item-7" *ngIf="contactActive" fxFlex="50%" fxFlex.lt-md="100%" fxFlexOrder="1">
            <!-- <h3 [ngClass.lt-md]="{'mob-txt':true}">Compare Quotes from 25+ Car Insurance Companies in India</h3>
               <h4 [ngClass.lt-md]="{'mob-txt':true}">One last step!</h4> -->
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 15px;
               font-weight: bold;">Compare Quotes from 25+ Car Insurance Companies in India</p>
            <p [ngClass.lt-md]="{'mob-txt':true}" style="font-size: 20px !important;
               text-align: left;
               color: #000;
               margin-top: 8px;
               font-weight: bold;">One last step!</p>
            <p class="motor_banner_p" fxHide="true" fxShow.gt-sm>Providing the correct details is very important. It will help us to fetch you customized car insurance quotes.</p>
         </div>
         <mat-horizontal-stepper fxFlexOrder="1" fxFlex="50%" style="width: 100%"
         (selectionChange)="selectionChange($event)" [linear]="isLinear" #stepper>
         <mat-step [stepControl]="brandFormGroup">
            <div class="topic">
               <!-- <h1>What's the brand of your car?</h1> -->
               <p style="font-size:16px !important;margin-top:4px;font-family:'Avenir-Book' !important;display:block;font-weight:bold;">What's the brand of your car?</p>
            </div>
            <div fxLayout="row" style="height: 80px;" fxLayoutAlign="center center"
               *ngIf="jsonLoader">
               <div id="registration-loader" style="text-align:center">
                  <div class="loader">Loading...</div>
               </div>
            </div>
            <form [formGroup]="brandFormGroup">
               <ng-template matStepLabel matTabContent>
                  <span [ngClass]="{'mat-step-label-selected-color':selectedStepperIndex!=1}"
                  [ngClass.lt-md]="{'hideme':true}" *ngIf="brandName?.length>12;else brandNameTooltip"
                  matTooltipClass="example-tooltip-red" matTooltip="{{brandName}}" [matTooltipPosition]="'above'"
                  class="example-button">{{brandName|slice:0:12}}..</span>
                  <ng-template #brandNameTooltip>
                     <div [ngClass]="{'mat-step-label-selected-color':selectedStepperIndex!=1}">{{brandName}}</div>
                  </ng-template>
               </ng-template>
               <div fxFlex="100%" fxFlexFill *ngIf="!jsonLoader">
                  <ng-select style="margin-top: 50px;padding-right: 10x;font-size: 13px;text-align: left;"
                  [virtualScroll]=true [items]="totalBrandList" bindLabel="name" (change)="brandChanged($event.id);track_button('create-brand-btn')"
                  dropdownPosition="bottom" [appendTo]="'body'" placeholder="Not in the list? Find your car's brand here">
                  </ng-select>
               </div>
            </form>
            <div class="good-make" id="top-brand" style="margin-top:23px" *ngIf="!jsonLoader">
               <div fxLayout="row wrap" fxLayoutGap="10px" fxLayoutAlign="center">
                  <ng-container *ngFor="let item of topBrandList">
                     <div class="img-wrap"
                        fxFlex="0 1 calc(20.3% - 5px)" fxFlex.lt-md="0 1 calc(40% - 10x)" fxLayout.lt-sm="row"
                        fxFlex.xs="30%" fxLayout.xs="column" fxLayout.sm="50%" fxLayout.md="50%" >
                        <a class="good-make-wrap" (click)="move(2);makeTapped(item.brand_name,item.brand_code,'',item.id);track_button('create-brand-btn')" href="JavaScript:void(0);" [ngClass]="{'is-selected':item.brand_code === form_brand}">
                        <div class="good-make-wrap" >
                           <img mat-card-image src='assets/quote/logo/{{item.brand_logo}}' alt='{{item.brand_name}}'>
                           <div class="brand-name">{{item.brand_name}}</div>
                        </div>
                        </a>
                     </div>
                  </ng-container>
               </div>
            </div>
         </mat-step>
         <mat-step [stepControl]="modelFormGroup" [editable]="isEditable">
         <a href="JavaScript:void(0);" (click)="goBackward()" area-label="back">
            <mat-icon aria-hidden="false" aria-label="Example keyboard_arrow_left icon">keyboard_arrow_left</mat-icon>
         </a>
         <div class="topic">
            <!-- <h1>What is your car’s model?</h1> -->
            <p style="font-size:16px !important;margin-top:4px;font-family:'Avenir-Book' !important;display:block;font-weight:bold;">What is your car’s model?</p>
         </div>
         <form [formGroup]="modelFormGroup">
            <ng-template matStepLabel matTabContent>
               <span [ngClass.lt-md]="{'hideme':true}" *ngIf="modelName.length>12;else modelNameTooltip"
               matTooltipClass="example-tooltip-red" matTooltip="{{modelName}}" [matTooltipPosition]="'above'"
               class="example-button">{{modelName| slice:0:12}}..</span>
               <ng-template #modelNameTooltip>
                  <div>{{modelName}}</div>
               </ng-template>
            </ng-template>
            <div fxFlex="100%" fxFlexFill>
               <ng-select #modelSelect style="margin-top: 50px;padding-right: 10x;font-size: 13px;text-align: left;"
               [virtualScroll]=true [items]="totalModelList" bindLabel="name"
               (change)="modelTapped($event.model_name,$event.model_code);track_button('create-model-btn');" dropdownPosition="bottom"
               [appendTo]="'body'" placeholder="Not in the list? Find your car's model here"></ng-select>
            </div>
         </form>
         <div class="good-make" id="top-model" style="margin-top: 10px" >
            <div fxLayout="row wrap" fxLayoutGap="10px" fxLayoutAlign="center" class="top-model__item" data-txt="AVEO"
               id="model_AVEO" data-val="undefined" style="margin-top:30px">
               <div *ngFor="let item of topModelList" [ngClass]="{'is-selected':item.model_code === form_model}"
               style="width: 110px" class="good-make-wrap img-wrap" fxFlex="0 1 calc(33% - 32px)"
               fxFlex.lt-md="0 1 calc(50% - 22px)" fxFlex.lt-sm="50%"
               (click)="move(3);modelTapped(item.model_name,item.model_code);track_button('create-model-btn')">
               <span class="example-button">{{item.model_name}}</span>
            </div>
         </div>
         <div fxLayout="row wrap" fxLayoutGap="10px" fxLayoutAlign="center" class="top-model__item">
            <div (click)="focusModle()" style="width: 100%" class="good-make-wrap img-wrap"
               fxFlex="0 1 calc(33% - 32px)" fxFlex.lt-md="0 1 calc(50% - 22px)" fxFlex.lt-sm="100%">
               Others
            </div>
         </div>
      </div>
      </mat-step>
      <mat-step [stepControl]="fuelFormGroup" [editable]="isEditable">
      <a href="JavaScript:void(0);" (click)="goBackward()" area-label="back">
         <mat-icon aria-hidden="false" aria-label="Example keyboard_arrow_left icon">keyboard_arrow_left</mat-icon>
      </a>
      <div class="topic">
         <!-- <h1>What's type of fuel ?</h1> -->
         <p style="font-size:16px !important;margin-top:4px;font-family:'Avenir-Book' !important;display:block;font-weight:bold;">What's type of fuel ?</p>
      </div>
      <form [formGroup]="fuelFormGroup">
         <ng-template matStepLabel matTabContent>
            <span [ngClass.lt-md]="{'hideme':true}" class="example-button">{{fuelName}}</span>
         </ng-template>
         <div class="good-make" id="Fuel">
            <div fxLayout="row wrap" fxLayout.lt-sm="column"  fxLayout.xs="row" fxLayoutGap="22px" fxLayoutAlign="center"
               class="top-model__item" data-txt="AVEO" id="model_AVEO" data-val="undefined">
               <div *ngFor="let item of totalFuelList" [ngClass]="{'is-selected':item.fuel_type_text === form_fuel}"
               class="good-make-wrap img-wrap" fxFlex="0 1 calc(22% - 32px)" fxFlex.lt-md="0 1 calc(50% - 22px)"
               fxFlex.lt-sm="100%" (click)="move(4);fuelTapped(item.fuel_type_text,item.fuel_type,item.model_code);track_button('create-fuel-btn')">
               <div class="good-make-wrap">
                  <img alt="Chevrolet" src="assets/quote/img/{{item.fuel_logo}}" alt="{{item.fuel_logo}}">
                  <div style="text-align: center">{{item.fuel_type_text}}</div>
               </div>
            </div>
         </div>
   </div>
   </form>
   </mat-step>
   <mat-step [stepControl]="variantFormGroup" [editable]="isEditable">
   <a href="JavaScript:void(0);" (click)="goBackward()" area-label="back">
      <mat-icon aria-hidden="false" aria-label="Example keyboard_arrow_left icon">keyboard_arrow_left</mat-icon>
   </a>
   <div class="topic">
      <!-- <h1>What's your car's variant?</h1>  -->
      <p style="font-size:16px !important;margin-top:4px;font-family:'Avenir-Book' !important;display:block;font-weight:bold;">What's your car's variant?</p>
   </div>
