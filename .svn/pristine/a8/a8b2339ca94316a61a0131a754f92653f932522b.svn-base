<header>
   <div fxFlexFill style="background: #055ba9;">
      <div fxLayout="row" class="container">
         <mat-toolbar color="primary">
            <header fxFlexFill fxFlex.lt-sm="100%">
               <div fxLayoutAlign="left" fxFlex="20" class="one" (click)="homepage()">
                  <img *ngIf="white_label==0" src="https://www.gibl.in/UI/assets/quote/images/logo.png" alt="logo"
                     height="67">
                  <span fxLayoutAlign="right" style="padding-left: 10px;"> CAR INSURANCE </span>
               </div>
               <div fxFlex fxLayout fxFlex="80" fxLayoutAlign="flex-end" style="float: right;" fxFlex="85" fxShow.gt-sm>
                  <div *ngIf="white_label==0" fxLayout="row" fxLayout.xs="column" fxShow.gt-sm>
                     <button target="_top" fxShow.gt-sm mat-button>email: info@gibl.in</button>
                     <button style="cursor: default;" fxShow.gt-sm mat-button fxHide.xs>Ph: +91 6289901320</button>
                  </div>
               </div>
               <ng-template #moo3>
                  <mat-dialog-actions>
                     <div fxLayout="row" fxFlex="100">
                        <button mat-button [matMenuTriggerFor]="menu">Support</button>
                        <button mat-button (click)="openDialog(moo3)">Login</button>
                        <button mat-button [matMenuTriggerFor]="menu">Register</button>
                     </div>
                  </mat-dialog-actions>
               </ng-template>
            </header>
         </mat-toolbar>
      </div>
   </div>
</header>
<mat-menu x-position="before" #menu="matMenu">
   <button mat-menu-item><a href="https://www.gibl.in/car-insurance/">Car Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/health-insurance/">Health Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/two-wheeler-insurance/">Two Wheeler Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/travel-insurance/">Travel Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/personal-accident-insurance/">Personal Accident
         Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/home-insurance/">Home Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/commercial-vehicle-insurance/">Commercial Vehicle
         Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/health-insurance/group-mediclaim-policy/">Group
         Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/health-insurance/critical-illness-insurance/">Critical Illness
         Insurance</a></button>
   <button mat-menu-item><a href="https://www.gibl.in/gadget-insurance/">Gadget Insurance</a></button>
</mat-menu>
<div class="container">
   <div fxLayout="row wrap" fxLayoutGap="10px" fxLayout="column" style="margin: auto;">
      <div fxLayoutAlign="center" style="flex-basis:100%;">
         <div *ngIf="showData" class="container no-padding" fxLayout="row" fxLayout.xs="column" fxFlexFill fxFlex=""
            fxFlex.xs="100">
            <div fxLayout.xs="row" fxFlex="100" fxLayout="row" style="flex-basis:50%;">
            </div>
         </div>
      </div>
      <div class="container space" fxLayout="row" fxLayout.xs="row" fxHide.lg fxHide.md fxHide.sm>
         <div fxFlex="30" class="sec3 insurer" fxFlex.xs="50" fxLayout.xs="column" fxFlex.sm="50"
            style=" background: #fff;padding:6px; " fxLayout="column">
            <div fxFlex="70" fxFlex.xs="100" class="sec2  logo-panel" fxLayout.xs="column">
               <h5>Insurer : <span>{{COMPANY_NAME}}</span></h5>
               <div fxFlex="100" style="text-align: center;">
                  <img style="max-width:80%; margin:auto;" src="assets/company_logo/{{COMPANY_LOGO}}"
                     alt="{{COMPANY_LOGO}}">
                  <!-- <a style="font-size:12px;text-align: center; margin: 0; display: inherit;" routerLink="/quote-compare" *ngIf="affiliateParam==''"> -->
                  <a style="font-size:12px;text-align: center; margin: 0;display: inherit;"
                     (click)="redirectTo('/quote-compare');">
                     <mat-icon class="no-icon" style="position:relative;" aria-hidden="false"
                        aria-label="Example keyboard_arrow_left icon">keyboard_arrow_left
                     </mat-icon>
                     <strong>Change Insurer</strong>
                  </a>
               </div>
            </div>
            <span class="material-icons" fxHide.xs style="font-size:50px; color: #055ba9;">
               directions_car
            </span>
            <p style="font-size:11px; text-align: center;">{{car_fullname}}</p>
         </div>
         <div class="space newpp" fxLayout="column" fxLayout.xs="column" fxLayoutAlign="left" fxLayoutGap="10px"
            fxLayoutGap.xs="0" fxFlex="38" fxFlex.xs="50">
            <div *ngIf="prev_policy_type=='Comprehensive'" class="item" fxFlex.xs="90">
               <span class="key" style="color: #fff;">IDV:
                  <small>Rs. {{idv | commaSeperator}} </small> </span>
            </div>
            <div class="item" fxFlex="35%" fxFlex.xs="100" style="margin-right:0px;">
               <span class="key">Premium:
                  <small style="font-size: 11px !important;">{{NET_PREMIUM | currency:'Rs. ' : 'symbol' : '1.0'}}
                  </small> </span><br>
               <span class="key">(+) GST: <small>{{SERVICE_TAX | currency:'Rs. ' : 'symbol' : '1.0'}}</small> </span>
            </div>
            <div class="item" style="float:right;place-content: flex-end;color: #055ba9 !important;margin-right:0px; "
               fxFlex="30%" fxFlex.xs="100"><span class="key"
                  style="color:#055ba9 !important;font-size:12px; font-weight:bold;">Total Premium: <small
                     style="color:#055ba9; font-size:10px ; font-weight:bold; padding-top:2px;">{{TOTAL_PREMIUM |
                     currency:'Rs. ' : 'symbol' : '1.0'}}</small> </span>
            </div>
         </div>
      </div>
      <div fxHide.xs>
         <div class="wrap-contact101 no-padding mt-10">
         </div>
         <div class="container space" fxLayout="row" fxLayout.xs="column">
            <div fxFlex="30" class="sec3 insurer" fxFlex.xs="50" fxLayout.xs="row" fxFlex.sm="50"
               style=" background: #fff;padding: 15px;" fxLayout="column">
               <img style="max-width:50%;" src="assets/company_logo/{{COMPANY_LOGO}}" alt="{{COMPANY_LOGO}}">
               <div fxFlex="70" fxFlex.xs="100" class="sec2  logo-panel" fxLayout.xs="column" style="margin-top:5px;">
                  <h5>Insurer : <span>{{COMPANY_NAME}}</span></h5>
                  <!-- <a style="font-size:12px;text-align: center; margin: 0;" routerLink="/quote-compare" *ngIf="affiliateParam==''"> -->
                  <a style="font-size:12px;text-align: center; margin: 0;" (click)="redirectTo('/quote-compare');">
                     <mat-icon class="no-icon" style="position:relative;" aria-hidden="false"
                        aria-label="Example keyboard_arrow_left icon">keyboard_arrow_left
                     </mat-icon>
                     <strong>Change Insurer</strong>
                  </a>
               </div>
            </div>
            <div fxFlex="30" class="sec3" fxFlex.xs="50" fxFlex.sm="50" fxLayout.xs="row" fxLayoutAlign="flex-start"
               fxLayout="column">
               <span class="material-icons" style="font-size:50px; color: #055ba9;">
                  directions_car
               </span>
               <p style="font-size: 13px">{{car_fullname}}</p>
            </div>
            <div class="space newpp" fxLayout="column" fxLayout.xs="column" fxLayoutAlign="left" fxLayoutGap="10px"
               fxLayoutGap.xs="0" fxFlex="38" fxFlex.xs="100">
               <div *ngIf="prev_policy_type=='Comprehensive'" class="item" fxFlex.xs="90"> <span class="key">IDV:
                     <small>Rs. {{idv | commaSeperator}} </small> </span>
               </div>
               <div class="item" fxFlex="35%" fxFlex.xs="100" style="margin-right:0px;">
                  <span>Premium:
                     <small>{{NET_PREMIUM | currency:'Rs. ' : 'symbol' : '1.0'}} </small> </span><br>
                  <span>(+) GST: <small>{{SERVICE_TAX | currency:'Rs. ' : 'symbol' : '1.0'}}</small> </span>
               </div>
               <div class="item"
                  style="float:right;place-content: flex-end;color: #055ba9 !important;margin-right:0px; " fxFlex="30%"
                  fxFlex.xs="100"><span style="color:#055ba9 !important; font-size:17px;">Total Premium: <small
                        style="color:#055ba9; font-size:17px;">{{TOTAL_PREMIUM | currency:'Rs. ' : 'symbol' :
                        '1.0'}}</small> </span>
               </div>
            </div>
         </div>
      </div>
      <div class="wrap-contact101  no-padding mt-10" style="background: transparent; box-shadow:none;">
         <mat-tab-group (selectedTabChange)="tabChanged($event)" [selectedIndex]="selectedIndex" #tabs>
            <mat-tab label="Your Personal Detail">
               <form [formGroup]="personalDetailForm" fxLayout="column" (ngSubmit)="personalDetailSubmit()">
                  <h2>Enter Your Personal Detail</h2>

                  <div fxFlex="100" fxLayout fxLayout.xs="column"
                     *ngIf="premiumJson?.PROVIDER_ID=='29' || premiumJson?.PROVIDER_ID=='28' || premiumJson?.PROVIDER_ID=='7' ">
                     <mat-radio-group formControlName="isOwnerCompany" aria-label="Select an option"
                        style="margin-top: 10px;">
                        <h2 style="display:flex;">Car Registered In Company Name?</h2>
                        <mat-radio-button (click)="isOwnerCompanyVm(1)" value="1">Yes</mat-radio-button>
                        <mat-radio-button (click)="isOwnerCompanyVm(0)" value="0">No</mat-radio-button>
                     </mat-radio-group>
                  </div>
                  <div fxFlex="100%" fxLayout="row" fxLayout.xs="row" *ngIf="isOwnerCompanyTitleShow">
                     <div fxFlex="20%" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-select formControlName="companyTitle" placeholder="Choose Title">
                              <mat-option value="M/S">M/S</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex="40%" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="nope" formControlName="companyName"
                              [ngModel]="proposalJson1?proposalJson1?.personalDetailForm?.companyName:null" matInput
                              placeholder="Enter Company Name">
                        </mat-form-field>
                     </div>
                     <div fxFlex="40%" class="pull-left" style="margin-right:20px;"
                        *ngIf="premiumJson?.PROVIDER_ID =='6' || premiumJson?.PROVIDER_ID =='28' ">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="nope"
                              formControlName="OrganizationContactPersonName"
                              [ngModel]="proposalJson1?proposalJson1?.personalDetailForm?.OrganizationContactPersonName:null"
                              matInput placeholder="{{name_place_holder}}">
                        </mat-form-field>
                     </div>

                  </div>


                  <div fxFlex="100%" fxLayout="row" fxLayout.xs="row" *ngIf="!isOwnerCompanyTitleShow">
                     <div fxFlex="20%" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-select formControlName="custTitle" placeholder="Choose Title">
                              <mat-option value="Mr.">Mr.</mat-option>
                              <mat-option value="Mrs.">Mrs.</mat-option>
                              <mat-option value="Ms.">Ms.</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex="80%" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="nope" formControlName="custName"
                              [ngModel]="refferJson?refferJson.cust_name:null" matInput
                              placeholder="{{name_place_holder}}">
                        </mat-form-field>
                     </div>
                  </div>
                  <h2 *ngIf="!isOwnerCompanyTitleShow">Select Date of Birth
                     <span *ngIf="showCustDobError" [ngStyle]="{'color': 'red'}">{{errorCustDobmsg}}</span>
                  </h2>


                  <div fxFlex="100%" *ngIf="!isOwnerCompanyTitleShow">
                     <div fxFlex.xs="24%" fxFlex="33%" fxLayout="row" class="pull-left" style="margin-right:10px;">
                        <mat-form-field>
                           <mat-select formControlName="custDOBDD" placeholder="Date"
                              (selectionChange)="checkAgeRestriction()">
                              <mat-option [value]="item" *ngFor="let item of custDOBDD">{{item}}</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex.xs="24%" fxFlex="33%" fxLayout="row" class="pull-left" style="margin-right:10px;">
                        <mat-form-field>
                           <mat-select formControlName="custDOBMM" placeholder="Month"
                              (selectionChange)="getMonthAndYear($event)">
                              <mat-option [value]="item" *ngFor="let item of custDOBMM">{{item}}</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex.xs="43%" fxFlex="33%" fxLayout="row" class="pull-left" style="margin-right:10px;">
                        <mat-form-field>
                           <mat-select formControlName="custDOBYY" placeholder="Year"
                              (selectionChange)="getMonthAndYear($event)">
                              <mat-option [value]="item" *ngFor="let item of custDOBYY">{{item}}</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <!--<span *ngIf="showCustDobError" class="invalid-feedback">
                  <span class="pull-left input-group formerror-msg">{{errorCustDobmsg}}</span>
                   </span>-->
                  </div>
                  <div fxFlex="100" fxLayout fxLayout.xs="column">
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="custGstNo"
                              matInput placeholder="Enter Your GST Number (Optional)">
                        </mat-form-field>
                     </div>
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" formControlName="custPancard" matInput
                              placeholder="{{pan_place_holder}}">
                        </mat-form-field>
                     </div>
                  </div>
                  <div fxFlex="100" fxLayout fxLayout.xs="column">
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input formControlName="custPhone" autocomplete="off" maxlength="10" matInput
                              placeholder="Enter Your Mobile Number">
                        </mat-form-field>
                     </div>
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input autocomplete="off" formControlName="custEmail" matInput
                              placeholder="Enter your email">
                        </mat-form-field>
                     </div>
                  </div>
                  <button (click)="track_button('personal-detail-btn')" type="submit" fxFlex="100"
                     class="contact100-form-btn" id="perionalDetailBtn">
                     <span>Continue to Your Car Detail
                        <i aria-hidden="true" class="fa fa-long-arrow-right m-l-7"></i>
                     </span>
                  </button>
               </form>
            </mat-tab>
            <mat-tab [disabled]="personalDetailForm.invalid" label="Your Car Detail">
               <form [formGroup]="carDetailForm" fxLayout="column" (ngSubmit)="carDetailSubmit()">
                  <h2>Enter Your Car Detail</h2>
                  <div fxFlex="100%" fxLayout fxLayout.xs="column">
                     <div fxFlex="50%" class="pull-left" style="margin-right:20px;margin-bottom: 15px">
                        <mat-form-field>
                           <mat-label>Enter Registration Number</mat-label>
                           <span matPrefix>{{rtoCodeTmp}} &nbsp;</span>
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="rtoregNumber"
                              matInput placeholder="placeholder">
                        </mat-form-field>
                     </div>
                  </div>
                  <div *ngIf="(isNew!=0 && PROVIDER_ID !='10') && (quoteJson?.dont_have_prev_policy==false)"
                     fxFlex="100" fxLayout fxLayout.xs="column">
                     <div fxFlex="50" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="prevPolicyNo"
                              matInput placeholder="Enter Previous Policy Number">
                        </mat-form-field>
                     </div>
                     <div fxFlex="50" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-label [ngClass]="{'setLabelRed':prevInsuErr==true}">Select Previous Insurer</mat-label>
                           <input autocomplete="off" matInput [formControl]="myControl" [matAutocomplete]="auto"
                              (input)="keyinputPrevInsurer($event.target.value)">
                           <mat-autocomplete #auto="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsPrevInsurer | async"
                                 [value]="option.name" (click)="setInsurer(option.name,option.id)">
                                 {{option.name}}
                              </mat-option>
                           </mat-autocomplete>
                        </mat-form-field>
                     </div>
                  </div>
                  <div *ngIf="isNew!=0 && PROVIDER_ID =='10'" fxFlex="100" fxLayout fxLayout.xs="column">
                     <div fxFlex="50" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="prevPolicyNo"
                              matInput placeholder="Enter Previous Policy Number">
                        </mat-form-field>
                     </div>
                     <div fxFlex="50" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-label [ngClass]="{'setLabelRed':prevInsuErr==true}">Select Previous Insurer</mat-label>
                           <input autocomplete="off" matInput [formControl]="myControl" [matAutocomplete]="auto"
                              (input)="keyinputPrevInsurer($event.target.value)">
                           <mat-autocomplete #auto="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsPrevInsurer | async"
                                 [value]="option.name" (click)="setInsurer(option.name,option.id)">
                                 {{option.name}}
                              </mat-option>
                           </mat-autocomplete>
                        </mat-form-field>
                     </div>
                     <!-- <div fxFlex="50" class="pull-left" style="margin-right:20px;">
                  
                  <mat-form-field>
                     <mat-label [ngClass]="{'setLabelRed':setNationalErr==true}">Select Previous Insurer Location</mat-label>
                     <input autocomplete="off" matInput [formControl]="myNationalControl" [matAutocomplete]="autonational"
                     (input)="resetNationalErr($event.target.value)">
                     <mat-autocomplete #autonational="matAutocomplete">
                        <mat-option *ngFor="let option of filteredOptionsNational | async" [value]="option.branch_name"
                        (click)="setNational(option.role_id,option.branch_name)">
                        {{option.branch_name}}
                        </mat-option>
                     </mat-autocomplete>
                  </mat-form-field>
               </div> -->
                  </div>
                  <div fxFlex="100" fxLayout fxLayout.xs="column">
                     <div fxFlex="50" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="engineNo"
                              matInput placeholder="Enter Car Enginee Number">
                        </mat-form-field>
                     </div>
                     <div fxFlex="50" class="pull-left" style="margin-right:20px;">
                        <mat-form-field *ngIf="PROVIDER_ID =='28' || PROVIDER_ID =='5' || PROVIDER_ID =='10'">
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="chassisNo"
                              minlength="17" maxlength="17" matInput placeholder="Enter Car 17 digit Chassis Number ">
                        </mat-form-field>
                        <mat-form-field *ngIf="PROVIDER_ID !='28' && PROVIDER_ID !='5' && PROVIDER_ID !='10'">
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="chassisNo"
                              maxlength="25" matInput placeholder="Enter Car Chassis Number ">
                        </mat-form-field>
                     </div>
                  </div>
                  <div fxFlex="100" fxLayout fxLayout.xs="column" *ngIf="PROVIDER_ID =='3'">
                     <mat-form-field>
                        <mat-select formControlName="pucStatus" placeholder="PUC Status">
                           <mat-option value="1">Yes</mat-option>
                           <mat-option value="0">No</mat-option>
                        </mat-select>
                        <mat-error *ngIf="c.pucStatus.errors?.puc_error">
                           Valid PUC required.
                        </mat-error>
                     </mat-form-field>
                  </div>
                  <div fxFlex="100" fxLayout fxLayout.xs="column">
                     <mat-radio-group formControlName="isLoan" name="radio" aria-label="Select an option"
                        style="margin-top: 10px;">
                        <h2 style="display:flex;">Is your car on loan?</h2>
                        <mat-radio-button (click)="isLoanVm(1)" value="1">Yes</mat-radio-button>
                        <mat-radio-button (click)="isLoanVm(0)" value="0">No</mat-radio-button>
                     </mat-radio-group>
                  </div>
                  <div
                     *ngIf="c.isLoan.value==1 && (premiumJson.PROVIDER_ID!='28' && premiumJson.PROVIDER_ID!='6' && premiumJson.PROVIDER_ID!='21' && premiumJson.PROVIDER_ID!='7'  && premiumJson.PROVIDER_ID!='4')"
                     fxFlex="100" fxLayout fxLayout.xs="column" style="margin-top:20px;">
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="financierName"
                              matInput placeholder="Name Of Financier">
                           <mat-hint align="start"><strong>Who is your car hypothecated to on RC?</strong> </mat-hint>
                        </mat-form-field>
                     </div>
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off"
                              formControlName="financierBranchName" matInput placeholder="Branch Name">
                           <mat-hint align="start"><strong>City where loan was taken ?</strong> </mat-hint>
                        </mat-form-field>
                     </div>
                  </div>
                  <div *ngIf="c.isLoan.value==1 && premiumJson.PROVIDER_ID=='28'" fxFlex="100" fxLayout
                     fxLayout.xs="column" style="margin-top:20px;">
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-select formControlName="financierName" placeholder="Choose Financier Name">
                              <mat-option *ngFor="let type of kotakJson"
                                 value="{{type.insurer_name}}~{{type.insurer_code}}">{{type.insurer_name}}</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off"
                              formControlName="financierBranchName" matInput placeholder="Branch Address">
                           <mat-hint align="start"><strong>City where loan was taken ?</strong> </mat-hint>
                        </mat-form-field>
                     </div>
                  </div>
                  <div *ngIf="c.isLoan.value==1 && premiumJson.PROVIDER_ID=='6'" fxFlex="100" fxLayout
                     fxLayout.xs="column" style="margin-top:20px;">
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-select formControlName="financierName" placeholder="Choose Financier Name">
                              <mat-option *ngFor="let type of hdfcJson" value="{{type.name}}">{{type.name}}</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                  </div>
                  <div *ngIf="c.isLoan.value==1 && premiumJson.PROVIDER_ID=='21'" fxFlex="100" fxLayout
                     fxLayout.xs="column" style="margin-top:20px;">
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-select formControlName="financierName" placeholder="Choose Financier Name">
                              <mat-option *ngFor="let type of totalMagmaHypoList"
                                 value="{{type.insurer_name}}~{{type.insurer_code}}">{{type.insurer_name}}</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off"
                              formControlName="financierBranchName" matInput placeholder="Branch Address">
                           <mat-hint align="start"><strong>City where loan was taken ?</strong> </mat-hint>
                        </mat-form-field>
                     </div>
                  </div>
                  <div *ngIf="c.isLoan.value==1 && (premiumJson.PROVIDER_ID=='7' || premiumJson.PROVIDER_ID=='4')"
                     fxFlex="100" fxLayout fxLayout.xs="column" style="margin-top:20px;">
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-select formControlName="financierName" placeholder="Choose Financier Name">
                              <mat-option *ngFor="let type of totalMagmaHypoList"
                                 value="{{type.insurer_name}}~{{type.insurer_code}}">{{type.insurer_name}}</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex="20 10 calc(40% - 30px)" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off"
                              formControlName="financierBranchName" matInput placeholder="Branch Address">
                           <mat-hint align="start"><strong>City where loan was taken ?</strong> </mat-hint>
                        </mat-form-field>
                     </div>
                  </div>
                  <button (click)="track_button('bike-detail-btn')" fxFlex="100" class="contact100-form-btn"
                     id="perionalDetailBtn" type="submit">
                     <span>Continue to Your Address
                        <i aria-hidden="true" class="fa fa-long-arrow-right m-l-7"></i>
                     </span>
                  </button>
               </form>
            </mat-tab>
            <mat-tab [disabled]="carDetailForm.invalid || personalDetailForm.invalid" label="Your Address">
               <form [formGroup]="addressDetailForm" fxLayout="column" (ngSubmit)="addressDetailSubmit()">
                  <h2>Enter Your Address</h2>
                  <div fxFlex="100" fxLayout fxLayout.xs="column">
                     <div fxFlex="100" class="pull-left">
                        <mat-form-field *ngIf="PROVIDER_ID =='29'">
                           <textarea style="text-transform: uppercase;" autocomplete="off" formControlName="custAddress"
                              matInput placeholder="Enter Your Current address for further communication"
                              maxlength="79"></textarea>
                        </mat-form-field>
                        <mat-form-field *ngIf="PROVIDER_ID !='29'">
                           <textarea style="text-transform: uppercase;" autocomplete="off" formControlName="custAddress"
                              matInput placeholder="Enter Your Current address for further communication"></textarea>
                        </mat-form-field>
                     </div>
                  </div>
                  <div fxFlex="100" fxLayout.xs="column">
                     <div fxFlex="30" fxFlex.xs="100" fxLayout="row" class="pull-left" style="margin-right: 15px;">
                        <mat-form-field>
                           <mat-label [ngClass]="{'setLabelRed':setStateErr==true}">Select State</mat-label>
                           <input autocomplete="off" matInput [formControl]="myStateControl" [matAutocomplete]="auto"
                              (input)="resetStateErr($event.target.value)">
                           <mat-autocomplete #auto="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsState | async" [value]="option.sname"
                                 (click)="setState(option.id,option.sname);getCity(option.id)">
                                 {{option.sname}}
                              </mat-option>
                           </mat-autocomplete>
                        </mat-form-field>
                     </div>
                     <div fxFlex="30%" fxLayout="row" class="pull-left" style="margin-right: 15px;" fxFlex.xs="100%">
                        <input type="hidden" formControlName="custCityLabel">
                        <mat-form-field>
                           <mat-label [ngClass]="{'setLabelRed':setCityErr==true}">Select City</mat-label>
                           <input autocomplete="off" matInput [formControl]="myCityControl" [matAutocomplete]="auto2"
                              (input)="resetCityErr($event.target.value)">
                           <mat-autocomplete #auto2="matAutocomplete">
                              <mat-option *ngFor="let option of filteredOptionsCity | async" [value]="option.city_name"
                                 (click)="setCity(option.city_id,option.city_name);">
                                 {{option.city_name}}
                              </mat-option>
                           </mat-autocomplete>
                        </mat-form-field>
                     </div>
                     <div fxFlex="30%" fxLayout="row" class="pull-left" fxFlex.xs="100%">
                        <mat-form-field>
                           <input autocomplete="off" formControlName="custPincode"
                              (input)="checkLength($event.target.value,6)" type="text" matInput
                              placeholder="Enter Your Pincode">
                        </mat-form-field>
                     </div>
                  </div>
                  <span class="error" *ngIf="pincodeValid">
                     Oops, this pincode does not exist!
                  </span>
                  <button (click)="track_button('address-detail-btn')" fxFlex="100" class="contact100-form-btn"
                     id="perionalDetailBtn" type="submit">
                     <span>Continue to Your Nominee Detail
                        <i aria-hidden="true" class="fa fa-long-arrow-right m-l-7"></i>
                     </span>
                  </button>
               </form>
            </mat-tab>
            <mat-tab [disabled]="addressDetailForm.invalid || carDetailForm.invalid || personalDetailForm.invalid"
               label="Nominee  Detail">
               <form [formGroup]="nomineeDetailForm" fxLayout="column" (ngSubmit)="nomineeDetailSubmit()">
                  <h2>Enter Your Nominee Detail</h2>
                  <div fxFlex="100%" fxLayout fxLayout.xs="row">
                     <div fxFlex="20%" fxFlex.xs="30%" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <mat-select formControlName="nomineeTitle" placeholder="Choose Title">
                              <mat-option value="Mr.">Mr.</mat-option>
                              <mat-option value="Mrs.">Mrs.</mat-option>
                              <mat-option value="Ms.">Ms.</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div fxFlex="80%" fxFlex.xs="70%" class="pull-left" style="margin-right:20px;">
                        <mat-form-field>
                           <input style="text-transform: uppercase;" autocomplete="off" formControlName="nomineeName"
                              matInput placeholder="Enter Nominee Name">
                        </mat-form-field>
                     </div>
                  </div>
                  <div fxFlex="100%" fxLayout.xs="column">
                     <div *ngIf="PROVIDER_ID !='21'" fxFlex="40%" fxLayout="row" class="pull-left"
                        style="margin-right:20px;" fxFlex.xs="100">
                        <mat-form-field>
                           <mat-select formControlName="nomineeRelation" placeholder="Choose Relationship">
                              <mat-option value="Father">Father</mat-option>
                              <mat-option value="Mother">Mother</mat-option>
                              <mat-option value="Daughter">Daughter</mat-option>
                              <mat-option value="Son">Son</mat-option>
                              <mat-option value="Spouse">Spouse</mat-option>
                              <mat-option value="Sibling">Sister</mat-option>
                              <mat-option value="Sibling">Brother</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div *ngIf="PROVIDER_ID =='21'" fxFlex="40%" fxLayout="row" class="pull-left"
                        style="margin-right:20px;" fxFlex.xs="100">
                        <mat-form-field>
                           <mat-select formControlName="nomineeRelation" placeholder="Choose Relationship">
                              <mat-option value="Father">Father</mat-option>
                              <mat-option value="Mother">Mother</mat-option>
                              <mat-option value="Daughter">Daughter</mat-option>
                              <mat-option value="Son">Son</mat-option>
                              <mat-option value="Spouse">Spouse</mat-option>
                              <mat-option value="Sister">Sister</mat-option>
                              <mat-option value="Brother">Brother</mat-option>
                           </mat-select>
                        </mat-form-field>
                     </div>
                     <div *ngIf="PROVIDER_ID !='21'" fxFlex="40%" fxLayout="row" class="pull-left"
                        style="margin-right:20px;" fxFlex.xs="100">
                        <mat-form-field>
                           <input autocomplete="off" (input)="checkLength($event.target.value,2)"
                              formControlName="nomineeAge" matInput placeholder="Enter nominee age" type="text">
                           <mat-error *ngIf="n.nomineeAge.invalid">
                              Nominee must be more than 18 years old
                           </mat-error>
                        </mat-form-field>
                     </div>
                     <div *ngIf="PROVIDER_ID == '21'" fxFlex="40%" fxLayout="row" class="pull-left"
                        style="margin-right:20px;" fxFlex.xs="100">
                        <div fxFlex="100%">
                           <!-- <span *ngIf="showMonineeDobError" [ngStyle]="{'color': 'red'}">Nominee must be more than 18 years old</span> -->
                           <div fxFlex.xs="24%" fxFlex="33%" fxLayout="row" class="pull-left"
                              style="margin-right:10px;">
                              <mat-form-field>
                                 <mat-select formControlName="nomineeDOBDD" placeholder="Date"
                                    (selectionChange)="checkNomineeAgeRestriction()">
                                    <mat-option [value]="item" *ngFor="let item of nomineeDOBDD">{{item}}</mat-option>
                                 </mat-select>
                              </mat-form-field>
                           </div>
                           <div fxFlex.xs="24%" fxFlex="33%" fxLayout="row" class="pull-left"
                              style="margin-right:10px;">
                              <mat-form-field>
                                 <mat-select formControlName="nomineeDOBMM" placeholder="Month"
                                    (selectionChange)="getMonthAndYearNominee($event)">
                                    <mat-option [value]="item" *ngFor="let item of nomineeDOBMM">{{item}}</mat-option>
                                 </mat-select>
                              </mat-form-field>
                           </div>
                           <div fxFlex.xs="43%" fxFlex="33%" fxLayout="row" class="pull-left"
                              style="margin-right:10px;">
                              <mat-form-field>
                                 <mat-select formControlName="nomineeDOBYY" placeholder="Year"
                                    (selectionChange)="getMonthAndYearNominee($event)">
                                    <mat-option [value]="item" *ngFor="let item of nomineeDOBYY">{{item}}</mat-option>
                                 </mat-select>
                              </mat-form-field>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div *ngIf="showNomineeDobError " fxFlex="100%" fxLayout="column" fxLayout.xs="row">
                     <h2>Enter Your Appointee Detail</h2>
                     <div fxLayout="row">
                        <div fxFlex="20%" fxLayout="row" class="pull-left" style="margin-right:20px;" fxFlex.xs="30%">
                           <mat-form-field>
                              <input style="text-transform: uppercase;" autocomplete="off"
                                 formControlName="appointeeName" matInput placeholder="Enter Appointee Name">
                           </mat-form-field>
                        </div>
                        <div fxFlex="20%" fxLayout="row" class="pull-left" style="margin-right:20px;" fxFlex.xs="30%">
                           <mat-form-field>
                              <mat-select formControlName="appointeeRelation" placeholder="Choose Relationship">
                                 <mat-option value="Father">Father</mat-option>
                                 <mat-option value="Mother">Mother</mat-option>
                                 <mat-option value="Daughter">Daughter</mat-option>
                                 <mat-option value="Son">Son</mat-option>
                                 <mat-option value="Spouse">Spouse</mat-option>
                                 <mat-option value="Sister">Sister</mat-option>
                                 <mat-option value="Brother">Brother</mat-option>
                              </mat-select>
                           </mat-form-field>
                        </div>
                        <div fxFlex="60%" fxLayout="row" class="pull-left" style="margin-right:20px;" fxFlex.xs="30%">
                           <div fxFlex.xs="50%" fxFlex="33%" fxLayout="row" class="pull-left"
                              style="margin-right:10px;">
                              <mat-form-field>
                                 <mat-select formControlName="appointeeDOBDD" placeholder="Date"
                                    (selectionChange)="checkAppointeeAgeRestriction()">
                                    <mat-option [value]="item" *ngFor="let item of appointeeDOBDD">{{item}}</mat-option>
                                 </mat-select>
                              </mat-form-field>
                           </div>
                           <div fxFlex.xs="50%" fxFlex="33%" fxLayout="row" class="pull-left"
                              style="margin-right:10px;">
                              <mat-form-field>
                                 <mat-select formControlName="appointeeDOBMM" placeholder="Month"
                                    (selectionChange)="getMonthAndYearAppointee($event)">
                                    <mat-option [value]="item" *ngFor="let item of appointeeDOBMM">{{item}}</mat-option>
                                 </mat-select>
                              </mat-form-field>
                           </div>
                           <div fxFlex.xs="50%" fxFlex="33%" fxLayout="row" class="pull-left"
                              style="margin-right:10px;">
                              <mat-form-field>
                                 <mat-select formControlName="appointeeDOBYY" placeholder="Year"
                                    (selectionChange)="getMonthAndYearAppointee($event)">
                                    <mat-option [value]="item" *ngFor="let item of appointeeDOBYY">{{item}}</mat-option>
                                 </mat-select>
                              </mat-form-field>
                           </div>
                           {{errorCustDobmsg}}
                        </div>
                     </div>
                  </div>
                  <span *ngIf="agediffDobError" [ngStyle]="{'color': 'red'}">Nominee age should not be greater then the
                     today's date.</span>
                  <button *ngIf="!nomineeDetailSubmitted" (click)="nextStep();track_button('nominee-detail-btn')"
                     fxFlex="100" class="contact100-form-btn" id="perionalDetailBtn" type="submit">
                     <span>Save & Continue
                        <i aria-hidden="true" class="fa fa-long-arrow-right m-l-7"></i>
                     </span>
                  </button>
                  <div class="wrab-div">
                     <div *ngIf="nomineeDetailSubmitted" class="loader-wrap">
                        <div class="loader">Loading...</div>
                     </div>
                  </div>
               </form>
            </mat-tab>
         </mat-tab-group>
      </div>
   </div>
</div>
