/*------------------------------------------------------------------
[ Alert validate ]*/

.validate-input {
  position: relative;
}

.alert-validate::before {
    content: attr(data-validate);
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    align-items: center;
    position: absolute;
    width: 100%;
    min-height: 41px;
    background-color: #fff;
    top: 17px;
    left: 0px;
    padding: 21px 45px 0 12px;
    pointer-events: none;
    font-size: 13px;
    color: #fa4251;
    line-height: 1.2;
    font-weight:400;
}

.btn-hide-validate {
    font-size: 18px;
    color: #fa4251;
    cursor: pointer;
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -ms-flexbox;
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    width: 40px;
    height: 40px;
    top:21px;
    right: 12px;
    font-weight: bold;
}



.rs1-alert-validate.alert-validate::before {
  background-color: #fff;
}

.true-validate::after {
  content: "\f058";
  font-family:'FontAwesome';
  font-size: 22px;
  color:#13d6a1;
  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  width: 40px;
  height: 40px;
  top: 27px;
  right: 10px;
}
.regis.true-validate::after{top: 8px;}
.name.true-validate::after{top: 8px;}
/*---------------------------------------------*/
@media (max-width: 576px) {
  .alert-validate::before {
    padding:21px 10px 0 10px;
  }

  .true-validate::after,
  .btn-hide-validate {
    right:13px;
    width: 30px;
  }
 .regis.true-validate::after, .btn-hide-validate{right: 13px;
    width: 30px; top:12px;}
	
	 .name.true-validate::after, .btn-hide-validate{right: 13px;
    width: 30px; top:12px;}
}


